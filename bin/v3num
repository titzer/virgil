#!/bin/bash

SOURCE="${BASH_SOURCE[0]}"
while [ -h "$SOURCE" ]; do
  BIN="$(builtin cd -P "$( dirname "$SOURCE" )" >/dev/null 2>&1 && pwd )"
  SOURCE="$(readlink "$SOURCE")"
  [[ $SOURCE != /* ]] && SOURCE="$BIN/$SOURCE"
done
BIN="$(builtin cd -P "$( dirname "$SOURCE" )" >/dev/null 2>&1 && pwd )"

$BIN/.build-util v3num apps/NumParse && exec $BIN/v3num "$@"
