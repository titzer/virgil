def UINT_TYPE = Int.getType(false, 32);
def LONG_TYPE = Int.getType(true, 64);

class VstNumberParserTester(t: Tester) {
	def test_implicit_int(val: int, dec: string, hex: string, bin: string) {
		y_int(dec, val);
		y_int(hex, val);
		y_int(bin, val);
	}
	def test_explicit_uint(val: int, dec: string, hex: string, bin: string) {
		y_uint(dec, val);
		y_uint(hex, val);
		y_uint(bin, val);
	}
	def y_int(str: string, val: int) {
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (VstNumberParser.getType(p) != Int.TYPE) return t.fail1("\"%s\" should be of type int", str);
		if (p.hasSuffix()) return t.fail1("\"%s\" should not have explicit type", str);
		var v = VstNumberParser.getVal(p);
		if (v == null) return t.fail1("\"%d\" was a null Val", val);
		if (!Box<int>.?(v)) return t.fail2("\"%s\" was not an int box, was %q", str, V3.renderResult(v, null, _));
		var r = Int.unbox(v);
		if (val != r) return t.fail2("\"%s\" parsed as %d", str, r);
	}
	def y_uint(str: string, val: int) {
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (VstNumberParser.getType(p) != UINT_TYPE) return t.fail1("\"%s\" should be of type int", str);
		if (!p.hasSuffix()) return t.fail1("\"%s\" should have explicit type", str);
		var v = VstNumberParser.getVal(p);
		if (v == null) return t.fail1("\"%d\" was a null Val", val);
		if (!Box<int>.?(v)) return t.fail2("\"%s\" was not an int box, was %q", str, V3.renderResult(v, null, _));
		var r = Int.unbox(v);
		if (val != r) return t.fail2("\"%s\" parsed as %d", str, r);
	}
	def test_type(str: string, ty: Type) {
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (VstNumberParser.getType(p) != ty) t.fail2("\"%s\" should be of type %q", str, ty.render);
	}
	def y_long(str: string, val: (int, int)) {
		var lval = long.view(val.0) << 32 | u32.view(val.1);
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (VstNumberParser.getType(p) != LONG_TYPE) return t.fail1("\"%s\" should be of type long", str);
		if (p.hasSuffix()) return t.fail1("\"%s\" should not have explicit type", str);
		var v = VstNumberParser.getVal(p);
		if (v == null) return t.fail1("\"%s\" parsed to a null Val", str);
		if (!Box<long>.?(v)) return t.fail2("\"%s\" was not an long box, was %q", str, V3.renderResult(v, null, _));
		var r = Box<long>.!(v).val;
		if (lval != r) return t.fail2("\"%s\" parsed as %q", str, StringBuilder.putd(_, r));
	}
	def y_longl(str: string, val: (int, int)) {
		var p = NumberParser.new(str);
		var lval = long.view(val.0) << 32 | u32.view(val.1);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (p.pos != str.length) return t.fail1("\"%s\" did not match parsed length", str);
		if (VstNumberParser.getType(p) != LONG_TYPE) return t.fail1("\"%s\" should be of type long", str);
		if (!p.hasSuffix()) return t.fail1("\"%s\" should have explicit type", str);
		var v = VstNumberParser.getVal(p);
		if (v == null) return t.fail1("\"%s\" parsed to a null Val", str);
		if (!Box<long>.?(v)) return t.fail2("\"%s\" was not an long box, was %q", str, V3.renderResult(v, null, _));
		var r = Box<long>.!(v).val;
		if (lval != r) return t.fail2("\"%s\" parsed as %d", str, r);
	}
	def assert_invalid(str: string) {
		var p = NumberParser.new(str);
		if (p.parse()) t.fail1("\"%s\" should not parse", str);
	}
	def assert_len(str: string, len: int) {
		var p = NumberParser.new(str);
		if (!p.parse() || p.pos != len) t.fail2("\"%s\" should parse of length %d", str, len);
	}

	def assert_f(str: string) {
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (!p.is_float) return t.fail1("\"%s\" should have .is_float=true", str);
		if (VstNumberParser.getType(p) != Float.FLOAT32) t.fail1("\"%s\" should be of type float", str);
	}
	def assert_d(str: string) {
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (!p.is_float) return t.fail1("\"%s\" should have .is_float=true", str);
		if (VstNumberParser.getType(p) != Float.FLOAT64) t.fail1("\"%s\" should be of type double", str);
	}
	def assert_fv(str: string, bits: u32) {
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (!p.is_float) return t.fail1("\"%s\" should have .is_float=true", str);
		if (VstNumberParser.getType(p) != Float.FLOAT32) return t.fail1("\"%s\" should be of type float", str);
		var v = VstNumberParser.getVal(p);
		if (!Float32Val.?(v)) return t.fail1("\"%s\" should be Float32Val", str);
		var got = Float32Val.!(v).bits;
		if (bits != got) return t.fail3("\"%s\" should be %x, got %x", str, bits, got);
	}
	def assert_dv(str: string, bits: u64) {
		var p = NumberParser.new(str);
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (!p.is_float) return t.fail1("\"%s\" should have .is_float=true", str);
		if (VstNumberParser.getType(p) != Float.FLOAT64) return t.fail1("\"%s\" should be of type double", str);
		var v = VstNumberParser.getVal(p);
		if (!Float64Val.?(v)) return t.fail1("\"%s\" should be Float64Val", str);
		var got = Float64Val.!(v).bits;
		if (bits != got) {
			var buf = StringBuilder.new();
			buf.put1("\"%s\" should be ", str);
			buf.putx(long.view(bits));
			buf.puts(", got ");
			buf.putx(long.view(got));
			return t.fail(buf.toString());
		}
	}
	def assert_int_type(str: string, signed: bool, width: byte) {
		var p = NumberParser.new(str);
		p.accept_width = true;
		if (!p.parse()) return t.fail1("\"%s\" did not parse", str);
		if (!IntType.?(VstNumberParser.getType(p))) return t.fail1("\"%s\" should be integral type", str);
		var it = IntType.!(VstNumberParser.getType(p));
		if (it.signed != signed || it.width != width) return t.fail3("\"%s\" expected type %q, got %q", str, Int.getType(signed, width).render, it.render);
	}

}

def testMisc(t: VstNumberParserTester) {
	def n = t.assert_invalid;
	n("-");

	var hex_buf = "0xx";
	var neg_buf = "-x";
	var pos_buf = "x";
	var alpha = "acbdefghijklmnopqrstuvABCDEFGHIJKLMNOPQRSTUVWXYZ";
	var symbol =  "`~!@#$%^&*()_=+[{]}|;:,<.>/?";
	var quote = "\\\'\"";
	for (s in [alpha, symbol, quote]) {
		for (ch in s) {
			neg_buf[1] = ch;
			n(neg_buf);
			pos_buf[0] = ch;
			n(pos_buf);
		}
	}

	for (ch in symbol) {
		hex_buf[2] = ch;
		n(hex_buf);
	}
	for (ch = 'G'; ch <= 'Z'; ch++) {
		hex_buf[2] = ch;
		n(hex_buf);
	}
	for (ch = 'g'; ch <= 'z'; ch++) {
		hex_buf[2] = ch;
		n(hex_buf);
	}
}

