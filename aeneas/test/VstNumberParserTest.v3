def testUnderscore(t: VstNumberParserTester) {
 	def T = t.test_implicit_int;

	T(0,			"0_",		"0x0_",		"0b0_");
	T(0,			"0_",		"0x0_00",	"0b0_000");
	T(1,			"1_",		"0x1_",		"0b1_");
	T(-1,		"-1_",		"0xf_fffffff",	"0b1_1111111111111111111111111111111");
	T(-2,		"-2_",		"0xff_fffffe",	"0b11_111111111111111111111111111110");
	T(-3,		"-3_",		"0xfff_ffffd",	"0b111_11111111111111111111111111101");
	T(-4,		"-4_",		"0xffff_fffc",	"0b1111_1111111111111111111111111100");
	T(-5,		"-5_",		"0xfffff_ffb",	"0b11111_111111111111111111111111011");
	T(-6,		"-6_",		"0xffffff_fa",	"0b111111_11111111111111111111111010");

	t.y_long("0x00_0000000123aF00", (0x00000000, 0x0123aF00));
	t.y_long("0x0000_0000_0123_aF00", (0x00000000, 0x0123aF00));

	def FV = t.assert_fv, DV = t.assert_dv;
	DV("1.818_357216161805012e-1", 0x3fc7466496cb03de);
	DV("1.8183572161_61805012e-1", 0x3fc7466496cb03de);
	DV("1_.81835721616180___5012e-1", 0x3fc7466496cb03de);
	DV("1.818357216161805012___e-1", 0x3fc7466496cb03de);
	DV("-4224062_6649973_3910300745403518.76181927_87047245958233565_750689494994394_67425687244e-300", 0x87cc905726c3abd8);

	FV("-758649_0734715.192621876706_151452080122799746379_8643716440736293925036482996157576791729202733568107336e-54f", 0x80001526);
}

def testWidth(t: VstNumberParserTester) {
	def L = t.assert_len;
	def T = t.assert_int_type;

	T("1",	true, 32);
	T("2u",	false, 32);
	T("3L",	true, 64);
	T("4uL",	false, 64);

	T("5u1",	false, 1);

	L("0u66", 2);
	L("0x0u66", 4);

	L("5U1", 2);
	L("5ul1", 3);
	L("5UL1", 3);
	L("5uL1", 3);
	L("3l22", 2);
	L("4L23", 2);
	L("-3l22", 3);
	L("-4L23", 3);

	T("6u5",	false, 5);
	T("7u17",	false, 17);
	T("7u31",	false, 31);
	T("8u32",	false, 32);
	T("9u41",	false, 41);
	T("12u63",	false, 63);
	T("13u64",	false, 64);

	T("0x5u1",	false, 1);
	T("0X6u5",	false, 5);
	T("0b01u17",	false, 17);
	T("0B10u17",	false, 17);

	T("-5i1",	true, 1);
	T("-6i5",	true, 5);
	T("-7i17",	true, 17);
	T("-7i31",	true, 31);
	T("-8i32",	true, 32);
	T("-9i41",	true, 41);
	T("-12i63",	true, 63);
	T("-13i64",	true, 64);

	T("5i1",	true, 1);
	T("6i5",	true, 5);
	T("7i17",	true, 17);
	T("7i31",	true, 31);
	T("8i32",	true, 32);
	T("9i41",	true, 41);
	T("12i63",	true, 63);
	T("13i64",	true, 64);

	T("0x5i1",	true, 1);
	T("0X6i5",	true, 5);
	T("0b01i17",	true, 17);
	T("0B10i17",	true, 17);
}
