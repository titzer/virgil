	var got = Float32Val.!(v).bits;
	if (expected != got) {
		var buf = StringBuffer.new();
		buf.format1("cast(%1) expected f32:", i);
		buf.putx(int.!(expected));
		buf.puts(", got ");
		buf.putx(int.!(got));
		TEST.error(buf.toString());
		return;
	}
}

