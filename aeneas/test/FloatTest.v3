def test_l_dn(i: long) {  // long -> double, expecting null
	var v = tryPromoteS_F64(i);
	if (v != null) {
		var got = Float64Val.!(v).bits;
		var buf = StringBuffer.new();
		buf.format1("cast(%1) expected null, got ", i);
		buf.putl(long.!(got));
		TEST.error(buf.toString());
	}
}
def test_l_dv(i: long, expected: u64) { // long -> double, expecting value
	var v = tryPromoteS_F64(i);
	if (v == null) {
		var buf = StringBuffer.new();
		buf.format1("cast(%1) expected f32:", i);
		buf.putl(long.!(expected));
		buf.puts(", got null");
		TEST.error(buf.toString());
		return;
	}
	var got = Float64Val.!(v).bits;
	if (expected != got) {
		var buf = StringBuffer.new();
		buf.format1("cast(%1) expected f32:", i);
		buf.putl(long.!(expected));
		buf.puts(", got ");
		buf.putl(long.!(got));
		TEST.error(buf.toString());
		return;
	}
}

