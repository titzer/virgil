			SUBI32 => code.inl_isx(tt.width);
			SUBU32 => code.inl_izx(tt.width);
			SUBI64 => code.inl_lsx(tt.width);
			SUBU64 => code.inl_lzx(tt.width);
			I32, U32, I64, U64 => ; // nop
		}
