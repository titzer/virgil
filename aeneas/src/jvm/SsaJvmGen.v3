					code.inl_u2l();
					code.l2d();
				}
				SUBI64, SUBU64, I64, U64 => code.l2d();
			}
		} else {
			match (ift.rank) {
				SUBI32, SUBU32, I32 => code.i2f();
				U32 => {
					code.inl_u2l();
					code.l2f();
				}
				SUBI64, SUBU64, I64, U64 => code.l2f();
			}
		}
	}
	def emitFloatCastI(i: SsaApplyOp, isDouble: bool) {
