var x = init(112, 112);
// TODO: fails because JVM backend cannot split heap init method
def init(r: int, c: int) -> Array<Array<float>> {
	var count = 1f;
	var result = Array<Array<float>>.new(r);
	for (i < r) {
		var a = Array<float>.new(c);
		result[i] = a;
		for (j < c) {
			a[j] = count;
			count *= 3.1f;
		}
	}
	return result;
}

def main(a: int) -> int {
	var sum = 0f;
	for (i < a) {
		for (j < a) {
			sum += x[i][j];
		}
	}
	return int.view(sum);
}
