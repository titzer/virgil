//@execute 0=false; 111=false; -981234=false
def main(a: int) -> bool {
	var f = float.!(a);

	// 24 live float values -> ensures spills on xmm
	var x1  = f +  1,  x2  = f +  2,  x3  = f +  3,  x4  = f +  4,  x5  = f +  5,  x6  = f +  6;
	var x7  = f +  7,  x8  = f +  8,  x9  = f +  9,  x10 = f + 10, x11 = f + 11, x12 = f + 12;
	var x13 = f + 13, x14 = f + 14, x15 = f + 15, x16 = f + 16, x17 = f + 17, x18 = f + 18;
	var x19 = f + 19, x20 = f + 20, x21 = f + 21, x22 = f + 22, x23 = f + 23, x24 = f + 24;

	// Compare f to many distinct values: all must be false (for any finite f)
	var c1  = f == x1,  c2  = f == x2,  c3  = f == x3,  c4  = f == x4,  c5  = f == x5,  c6  = f == x6;
	var c7  = f == x7,  c8  = f == x8,  c9  = f == x9,  c10 = f == x10, c11 = f == x11, c12 = f == x12;
	var c13 = f == x13, c14 = f == x14, c15 = f == x15, c16 = f == x16, c17 = f == x17, c18 = f == x18;
	var c19 = f == x19, c20 = f == x20, c21 = f == x21, c22 = f == x22, c23 = f == x23, c24 = f == x24;

	return c1 & c2 & c3 & c4 & c5 & c6 & c7 & c8 & c9 & c10 &
	       c11 & c12 & c13 & c14 & c15 & c16 & c17 & c18 & c19 & c20 &
	       c21 & c22 & c23 & c24;
}
