//@execute 0=28; 3=31; 8=36; 14=42
def main(a: int) -> int {
	var v0 = 0f;
	var v1 = 1f;
	var v2 = 2f;
	var v3 = 3f;
	var v4 = 4f;
	var v5 = 5f;
	var v6 = 6f;
	var v7 = 7f;
	
	for (i < a) {
		match (i & 7) {
			0 => v0 += 1f;
			1 => v1 += 1f;
			2 => v2 += 1f;
			3 => v3 += 1f;
			4 => v4 += 1f;
			5 => v5 += 1f;
			6 => v6 += 1f;
			7 => v7 += 1f;
		}
	}
	var s = v0 + v1 + v2 + v3 + v4 + v5 + v6 + v7;
	return int.truncf(s);
}
