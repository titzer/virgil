//@execute 0=28; 3=31; 8=36; 14=42
def main(a: int) -> int {
	var v0 = 0d;
	var v1 = 1d;
	var v2 = 2d;
	var v3 = 3d;
	var v4 = 4d;
	var v5 = 5d;
	var v6 = 6d;
	var v7 = 7d;
	
	for (i < a) {
		match (i & 7) {
			0 => v0 += 1d;
			1 => v1 += 1d;
			2 => v2 += 1d;
			3 => v3 += 1d;
			4 => v4 += 1d;
			5 => v5 += 1d;
			6 => v6 += 1d;
			7 => v7 += 1d;
		}
	}
	var s = v0 + v1 + v2 + v3 + v4 + v5 + v6 + v7;
	return int.truncd(s);
}
