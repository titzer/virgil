//@execute 0=false; 5=false; -5=false
def main(a: int) -> bool {
	var f = float.!(a);

	// Make a bunch of NaNs via 0.0/0.0 patterns that keep many temps live.
	var z0 = f - f;               // 0.0 for finite f
	var n1 = z0 / z0;
	var n2 = (z0 + 1.0) - (z0 + 1.0); // 0.0 -> NaN on divide
	var n3 = n2 / z0;
	var n4 = n1 + n2;
	var n5 = n3 + n4;
	var n6 = n5 - n1;
	var n7 = n6 + n2;
	var n8 = n7 - n3;
	var n9 = n8 + n4;
	var n10 = n9 - n5;
	var n11 = n10 + n6;
	var n12 = n11 - n7;

	// Compare each NaN with some numbers and each other; all false.
	var c1  = n1  == f + 1,  c2  = n2  == f + 2,  c3  = n3  == f + 3,  c4  = n4  == f + 4;
	var c5  = n5  == f + 5,  c6  = n6  == f + 6,  c7  = n7  == f + 7,  c8  = n8  == f + 8;
	var c9  = n9  == f + 9,  c10 = n10 == f + 10, c11 = n11 == f + 11, c12 = n12 == f + 12;

	return c1 & c2 & c3 & c4 & c5 & c6 & c7 & c8 & c9 & c10 & c11 & c12;
}
