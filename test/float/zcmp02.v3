//@execute 0=false; 5=false; -7=false
def main(a: int) -> bool {
	var f = float.!(a);
	var y1  = f +  0, y2  = f +  1, y3  = f +  2, y4  = f +  3, y5  = f +  4, y6  = f +  5;
	var y7  = f +  6, y8  = f +  7, y9  = f +  8, y10 = f +  9, y11 = f + 10, y12 = f + 11;
	var y13 = f + 12, y14 = f + 13, y15 = f + 14, y16 = f + 15, y17 = f + 16, y18 = f + 17;
	var y19 = f + 18, y20 = f + 19, y21 = f + 20, y22 = f + 21, y23 = f + 22, y24 = f + 23;

	// Compare neighbors (all distinct)
	var c1  = y1  == y2,  c2  = y2  == y3,  c3  = y3  == y4,  c4  = y4  == y5,  c5  = y5  == y6,  c6  = y6  == y7;
	var c7  = y7  == y8,  c8  = y8  == y9,  c9  = y9  == y10, c10 = y10 == y11, c11 = y11 == y12, c12 = y12 == y13;
	var c13 = y13 == y14, c14 = y14 == y15, c15 = y15 == y16, c16 = y16 == y17, c17 = y17 == y18, c18 = y18 == y19;
	var c19 = y19 == y20, c20 = y20 == y21, c21 = y21 == y22, c22 = y22 == y23, c23 = y23 == y24;

	return c1 & c2 & c3 & c4 & c5 & c6 & c7 & c8 & c9 & c10 &
	       c11 & c12 & c13 & c14 & c15 & c16 & c17 & c18 & c19 & c20 &
	       c21 & c22 & c23;
}
