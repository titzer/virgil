//@execute = 42
def f64_bits(exp: u32, mantissa: u64) -> u64 {
	if (exp > 0) {
		mantissa = 0x1_0000_0000uL | mantissa >> 1;
		mantissa = mantissa >> u6.view(exp - 1);
		exp = 0;
	}
	var bits = u64.view(exp) << 52;
	bits |= mantissa >> 12;
	return bits;
}
def main() -> int {
	var result = f64_bits(1, 0);
	if (result != 0x0010_0000) return -1;
	return 42;
}
