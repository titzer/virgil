//@execute 0=42; 44444=42; 100000=42
def bad(exp: u32, mantissa: u64) -> u64 {
	if (exp > 0) {
		mantissa = 0x1_0000_0000uL | mantissa >> 1;
		mantissa = mantissa >> u6.view(exp - 1);
		exp = 0;
	}
	var bits = u64.view(exp) << 52;
	bits |= mantissa >> 12;
	return bits;
}
def main(a: u32) -> int {
	var result = bad(1, a);
	if (result != (0x0010_0000u | (a >> 13))) return -1;
	return 42;
}
