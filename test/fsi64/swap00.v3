//@execute 0=24; 1=3; 3=9; 100=324; 101=303; 102=334; 103=309
def foo(a: u32) -> (u32, u64) {
	var b = u64.view(a);
	b |= b << 32;
	match (a & 0x3) {
		0 => { a += 12; }
		1 => { b += 13; }
		2 => { a += 14; b += 16; }
		3 => { }
	}
	return (a, b);
}
def main(a: u32) -> u32 {
	var x = foo(a);
	var r = x.0 + u32.view(x.1 >> 32) + u32.view(x.0);
	return r;
}