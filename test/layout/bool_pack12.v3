//@execute (false, 0, 0)=0; (false, 0, 1)=1; (false, 1, 0)=2; (false, 1, 1)=3; (true, 0, 0)=4; (true, 0, 1)=5; (true, 1, 0)=6; (true, 1, 1)=7
def main(a: bool, b: u1, c: u1) -> int {
	var x = (u4.view(a) << 2) | (u4.view(b) << 1) | (u4.view(c));

	if (x != u3.pack((a, b, c))) return -11;
	if (x != u3.pack(((a, b), c))) return -12;
	if (x != u3.pack((a, (b, c)))) return -13;

	return x;
}
