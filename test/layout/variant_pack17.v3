//@execute = 42
//@heap-size=10000
type Nibbles<T>(x: T, y: T) #unboxed;
def main() -> int {
	for (i < 16) {
		for (j < 16) {
			if (!check(Nibbles(u4.view(i), u4.view(j)))) return 0 - i * 16 - j;
		}
	}
	return 42;
}
def check(n: Nibbles<u4>) -> bool {
	var expect = (u8.view(n.x) << 4) | u8.view(n.y);
	var got = u8.pack(n);
	return expect == got;
}
