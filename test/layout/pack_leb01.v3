//@execute 0=0; 1=1; 127=127; 128=384; 2788=5604; 12980=26036; 65001=261097; 65535=262143; 100000=429472; 200000=826048; 1000000=4031680; 10000000=81964416; 16777215=134217727
def main(x: u24) -> u32 {
	var x0 = u7.view(x);
	var x1 = u7.view(x >> 7);
	var x2 = u7.view(x >> 14);
	var x3 = u7.view(x >> 21);

	if (x <= u7.max) {
		return x0;
	} else if (x <= u14.max) {
		var b0 = u8.pack((1u1, x0));
		var b1 = u8.pack((0u1, x1));
		return u16.pack((b1, b0));
	} else if (x <= u21.max) {
		var b0 = u8.pack((1u1, x0));
		var b1 = u8.pack((1u1, x1));
		var b2 = u8.pack((0u1, x2));
		return u24.pack((b2, b1, b0));
	} else {
		var b0 = u8.pack((1u1, x0));
		var b1 = u8.pack((1u1, x1));
		var b2 = u8.pack((1u1, x2));
		var b3 = u8.pack((0u1, x3));
		return u32.pack((b3, b2, b1, b0));
	}
}
