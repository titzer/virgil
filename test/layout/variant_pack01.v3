//@execute = 42
type Crumbs(a: u2, b: u2, c: u2, d: u2);
def main() -> int {
	for (x < 4) {
		for (y < 4) {
			for (z < 4) {
				for (w < 4) {
					if (!check(u2.view(x), u2.view(y), u2.view(z), u2.view(w))) return -119;
				}
			}
		}
	}
	return 42;
}
def check(x: u2, y: u2, z: u2, w: u2) -> bool {
	var expect = (u8.view(x) << 6) | (u8.view(y) << 4) | (u8.view(z) << 2) | u8.view(w);
	var got = u8.pack(Crumbs(x, y, z, w));
	return expect == got;
}
