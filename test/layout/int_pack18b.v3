//@execute (0, 0, 0)=0; (0, 0, 1)=1; (0, 1, 0)=2; (0, 1, 1)=3; (1, 0, 0)=4; (1, 0, 1)=5; (1, 1, 0)=6; (1, 1, 1)=7
def main(a: u1, b: u1, c: u1) -> int {
	var x = (u4.view(a) << 2) | (u4.view(b) << 1) | (u4.view(c));

	if (x != u3.pack((a, b, c))) return -11;
	if (x != u3.pack((pair(a, b), c))) return -12;
	if (x != u3.pack((a, pair(b, c)))) return -13;

	return x;
}
def pair<T>(x: T, y: T) -> (T, T) {
	return (x, y);
}