//@execute (0, 0)=0; (1, 1)=17; (15, 15)=255; (10, 1)=161
type Crumb(x: u2) #unboxed;
type Nibble(l: Crumb, r: Crumb) #unboxed;
type Nibbles(l: Nibble, r: Nibble) #unboxed;
def main(a: u4, b: u4) -> u8 {
	return u8.pack(
		Nibbles(
			Nibble(Crumb(u2.view(a >> 2)), Crumb(u2.view(a))),
			Nibble(Crumb(u2.view(b >> 2)), Crumb(u2.view(b)))
		)
	);
}
