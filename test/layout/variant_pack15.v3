//@execute = 42
type Nibbles<T>(x: u4, y: T);
def main() -> int {
	for (i < 16) {
		for (j < 16) {
			if (!check(u4.view(i), u4.view(j))) return 0 - i * 16 - j;
		}
	}
	return 42;
}
def check(x: u4, y: u4) -> bool {
	var expect = (u8.view(x) << 4) | u8.view(y);
	var got = u8.pack(Nibbles<u4>(x, y));
	return expect == got;
}
