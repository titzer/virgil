//@execute = 42
//@heap-size=10000
type Crumbs<T>(a: u2, b: u2, c: T, d: u2);
def main() -> int {
	for (x < 4) {
		for (y < 4) {
			for (z < 4) {
				for (w < 4) {
					if (!check(Crumbs(u2.view(x), u2.view(y), (), u2.view(w)))) return -119;
				}
			}
		}
	}
	return 42;
}
def check(c: Crumbs<void>) -> bool {
	var expect = (u8.view(c.a) << 4) | (u8.view(c.b) << 2) | u8.view(c.d);
	var got = u8.pack(c);
	return expect == got;
}
