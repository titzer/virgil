//@execute 0=false; 1=true; 2=false; 3=true; 4=true; 5=false; 6=false; 7=false

component const03 {
    var a: bool = true;
    var b: bool = false;

    def main(arg: int) -> bool {
	if (arg == 1) return a;
	if (arg == 2) return b;
	if (arg == 3) return a == true;
	if (arg == 4) return b == false;
	if (arg == 5) return a == false;
	if (arg == 6) return b == true;
	return false;
    }
}
