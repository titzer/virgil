//@execute 0=0; 1=13; 2=14; 3=15; 4=16; 5=17; 6=18; 7=0

component array22 {
    var a: Array<int> = Array<int>.new(4);
    var b: Array<int> = Array<int>.new(6);

    def fill(a: Array<int>, v: int) {
	var i = 0;
	while (i < a.length) {
		a(i) = v;
		i = i + 1;
	}
    }

    def main(arg: int) -> int {
	fill(a, 0);
	fill(b, 0);
	if (arg == 1) fill(a, 13);
	if (arg == 2) fill(b, 14);
	if (arg == 3) fill(a, 15);
	if (arg == 4) fill(b, 16);
	if (arg == 5) fill(a, 17);
	if (arg == 6) fill(b, 18);
	return a(a.length - 1) + b(b.length - 1);
    }
}
