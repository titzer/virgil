//@execute 0=32769; 1=16385; 2=0
def main(a: int) -> int {
	def p = int.pack<(u16,u15,bool)>((0b1000000000000001,0b100000000000001,false));
	def u: (u16,u15,bool) = int.unpack<(u16,u15,bool)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1;
		2 => return if(u.2, 1, 0);
		_ => return 42;
	}
}
