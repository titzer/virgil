//@execute 0=1; 1=-2; 2=1; 3=3; 4=-4; 5=0; 6=5; 7=1; 8=-6; 9=7; 10=0; 11=-8; 12=1; 13=9; 14=-10; 15=0; 16=11; 17=-12
type C1(a: i12, b: bool) #unboxed #packed {}
type C2(a: bool, b: i12) #unboxed #packed {}
type C3(a: u17, b: i12) #unboxed #packed {}
type A #unboxed #packed {
    case One(a: u17, b: C1) #unboxed #packed;
    case Two(a: u17, b: C2) #unboxed #packed;
    case Three(a: bool, b: C3) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(1, C1(-2, true)), 
    A.One(3, C1(-4, false)), 
    A.Two(5, C2(true, -6)), 
    A.Two(7, C2(false, -8)), 
    A.Three(true,  C3(9, -10)), 
    A.Three(false, C3(11, -12))
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return A.One.!(arr[0]).b.a;
        2 => return int.view<bool>(A.One.!(arr[0]).b.b);

        3 => return A.One.!(arr[1]).a;
        4 => return A.One.!(arr[1]).b.a;
        5 => return int.view<bool>(A.One.!(arr[1]).b.b);

        6 => return A.Two.!(arr[2]).a;
        7 => return int.view<bool>(A.Two.!(arr[2]).b.a);
        8 => return A.Two.!(arr[2]).b.b;

        9 => return A.Two.!(arr[3]).a;
        10 => return int.view<bool>(A.Two.!(arr[3]).b.a);
        11 => return A.Two.!(arr[3]).b.b;

        12 => return int.view<bool>(A.Three.!(arr[4]).a);
        13 => return A.Three.!(arr[4]).b.a;
        14 => return A.Three.!(arr[4]).b.b;

        15 => return int.view<bool>(A.Three.!(arr[5]).a);
        16 => return A.Three.!(arr[5]).b.a;
        17 => return A.Three.!(arr[5]).b.b;

        _ => return 42;
    }
}