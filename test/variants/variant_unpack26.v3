//@execute 1=3; 2=5; 3=5; 4=1; 5=3
enum E {
    A, B, C
}
type C (i1: u2, i2: (u3, E.set, u3), i3: u2){ }

def main(a: int) -> int {

    def e = E.A|E.C; 
    def origT: (u3, E.set, u3) = (0b101, e, 0b101);
    def c: C = C(0b11, origT, 0b11);
    def u: C = int.unpack<C>(int.pack<C>(c));
    def newT: (u3, E.set, u3) = u.i2;

    match (a) {
        1 => return u.i1;
        2 => return u.i2.0;
        3 => return u.i2.2;
        4 => return if(u.i2.1 == e, 1, 0);
        5 => return u.i3;
        _ => return 42;
    }
}