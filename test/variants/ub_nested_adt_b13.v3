//@execute 0=1; 1=2; 2=10; 3=1; 4=3; 5=11; 6=10; 7=2; 8=1; 9=20; 10=3; 11=1; 12=1; 13=1
type C1(a: float, b: u4) #unboxed #packed {}
type C2(a: u3, b: float) #unboxed #packed {}
type A #unboxed #packed {
    case One(a: C1, c: u23) #unboxed #packed;
    case Two(a: u24, b: C2) #unboxed #packed;
    case Three(a: float) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(C1(1F, 2), 10), 
    A.One(C1(4F, 3), 11), 
    A.Two(10, C2(2, 7F)), 
    A.Two(20, C2(3, 10F)), 
    A.Three(11F), 
    A.Three(12F)
];

def main(a: int) -> int {
    match(a) {
        0 => return if(A.One.!(arr[0]).a.a == 1F, 1, 0);
        1 => return A.One.!(arr[0]).a.b;
        2 => return A.One.!(arr[0]).c;

        3 => return if(A.One.!(arr[1]).a.a == 4F, 1, 0);
        4 => return A.One.!(arr[1]).a.b;
        5 => return A.One.!(arr[1]).c;

        6 => return A.Two.!(arr[2]).a;
        7 => return A.Two.!(arr[2]).b.a;
        8 => return if(A.Two.!(arr[2]).b.b == 7F, 1, 0);

        9 => return A.Two.!(arr[3]).a;
        10 => return A.Two.!(arr[3]).b.a;
        11 => return if(A.Two.!(arr[3]).b.b == 10F, 1, 0);

        12 => return if(A.Three.!(arr[4]).a == 11F, 1, 0);

        13 => return if(A.Three.!(arr[5]).a == 12F, 1, 0);
        _ => return 42;
    }
}