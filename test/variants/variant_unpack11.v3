//@execute 0=129; 1=3; 2=3; 3=3; 4=129; 5=3; 6=3; 7=3; 8=129
type C1 (i1: byte, i2: u2, i3: u2){ }
type C2 (i1: u2, i2: byte, i3: u2){ }
type C3 (i1: u2, i2: u2, i3: byte){ }
def main(a: int) -> int {
    def c1: C1 = C1(0b10000001, 0b11, 0b11);
    def c2: C2 = C2(0b11, 0b10000001, 0b11);
    def c3: C3 = C3(0b11, 0b11, 0b10000001);

    def u1 = int.unpack<C1>(int.pack<C1>(c1));
    def u2 = int.unpack<C2>(int.pack<C2>(c2));
    def u3 = int.unpack<C3>(int.pack<C3>(c3));

    match (a) {
        0 => return u1.i1; 
        1 => return u1.i2;
        2 => return u1.i3;
        3 => return u2.i1; 
        4 => return u2.i2;
        5 => return u2.i3;
        6 => return u3.i1; 
        7 => return u3.i2;
        8 => return u3.i3;
        _ => return 42;
    }
}