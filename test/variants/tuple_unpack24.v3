//@execute 0=0; 1=129; 2=129; 3=65; 4=129
def main(a: int) -> int {
	def p = int.pack<((bool,u8,u8),u7,u8)>(((false,0b10000001,0b10000001),0b1000001,0b10000001));
	def u: ((bool,u8,u8),u7,u8) = int.unpack<((bool,u8,u8),u7,u8)>(p);
	match (a) {
		0 => return if(u.0.0, 1, 0); 
		1 => return u.0.1; 
		2 => return u.0.2; 
		3 => return u.1;
		4 => return u.2;
		_ => return 42;
	}
}
