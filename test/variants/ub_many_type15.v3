//@execute 0=1; 1=-2; 2=1; 3=3; 4=-4; 5=1; 6=-5; 7=6; 8=1; 9=-7; 10=8; 11=1
type A #unboxed #packed {
    case One(a: u17, b: i14, c: float) #unboxed #packed;
    case Two(a: i18, b: u13, c: float) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(1, -2, 1F), 
    A.One(3, -4, 2F), 
    A.Two(-5, 6, 1F), 
    A.Two(-7, 8, 2F)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return A.One.!(arr[0]).b;
        2 => return if(A.One.!(arr[0]).c == 1F, 1, 0);

        3 => return A.One.!(arr[1]).a;
        4 => return A.One.!(arr[1]).b;
        5 => return if(A.One.!(arr[1]).c == 2F, 1, 0);

        6 => return A.Two.!(arr[2]).a;
        7 => return A.Two.!(arr[2]).b;
        8 => return if(A.Two.!(arr[2]).c == 1F, 1, 0);

        9 => return A.Two.!(arr[3]).a;
        10 => return A.Two.!(arr[3]).b;
        11 => return if(A.Two.!(arr[3]).c == 2F, 1, 0);
        _ => return 42;
    }
}