//@execute 0=21; 1=37; 2=15; 3=19
type A #unboxed #packed {
    case One(a: (u4, u8, u3), b: (u4, u8, u3)) #unboxed #packed;
    case Two(a: u16, b: u15) #unboxed #packed;
}
def arr = [
    A.One((1, 2, 3), (4, 5, 6)), 
    A.One((4, 5, 6), (7, 8, 7)), 
    A.Two(7, 8), 
    A.Two(9, 10)
];

def main(a: int) -> int {
    match(arr[a]) {
        One(a, b) => return a.0 + a.1 + a.2 + b.0 + b.1 + b.2;
        Two(a, b) => return a + b;
    }
}