//@execute 0=1; 1=2; 2=3; 3=0; 4=5; 5=1; 6=1; 7=8; 8=0; 9=10
type A #unboxed #packed {
    case One(a: bool, b: u16, c: u7) #unboxed #packed;
    case Two(a: bool, b: u15) #unboxed #packed;
}
def arr: Array<A> = [
    A.One(true, 2, 3), 
    A.One(false, 5, 1), 
    A.Two(true, 8), 
    A.Two(false, 10)
];

def main(a: int) -> int {
    match(a) {
        0 => return int.view<bool>(A.One.!(arr[0]).a);
        1 => return A.One.!(arr[0]).b;
        2 => return A.One.!(arr[0]).c;

        3 => return int.view<bool>(A.One.!(arr[1]).a);
        4 => return A.One.!(arr[1]).b;
        5 => return A.One.!(arr[1]).c;

        6 => return int.view<bool>(A.Two.!(arr[2]).a);
        7 => return A.Two.!(arr[2]).b;

        8 => return int.view<bool>(A.Two.!(arr[3]).a);
        9 => return A.Two.!(arr[3]).b;
        _ => return 42;
    }
}