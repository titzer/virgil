//@execute 0=1; 1=129; 2=129; 3=33; 4=65
enum E { A, B, C }
def main(a: int) -> int {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = int.pack<((E.set,u8,u8),u6,u7)>(((e5,0b10000001,0b10000001),0b100001,0b1000001));
	def u: ((E.set,u8,u8),u6,u7) = int.unpack<((E.set,u8,u8),u6,u7)>(p);
	match (a) {	
		0 => return if(u.0.0==e5, 1, 0);
		1 => return u.0.1;
		2 => return u.0.2;
		3 => return u.1;
		4 => return u.2;
		_ => return 42;
	}
}
