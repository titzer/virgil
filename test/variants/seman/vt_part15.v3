type Y<N> {
	def m(a: N, b: byte) -> bool { return false; }
}
def y: Y<int>;

def a: (Y<int>, int, byte) -> bool = Y.m(_, _, _);

def b: (Y<int>, int) -> bool    = Y.m(_, _, 'a');
def c: (Y<int>, byte) -> bool   = Y.m(_, 1, _);
def d: (int, byte) -> bool = Y.m(y, _, _);

def e: Y<int> -> bool    = Y.m(_, 2, '3');
def f: int -> bool  = Y.m(y, _, '4');
def g: byte -> bool = Y.m(y, 4, _);
