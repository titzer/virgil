type Sum {
	case Int(i: int);
	case Byte(b: byte);
	case Bool(b: bool);
	case Void(v: void);
}
def printInt(i: int);
def printByte(b: byte);
def printBool(b: bool);
def printVoid(v: void);
def id(t: Sum) {
	match (t) {
		Int(i) => return printInt(i);
		Byte(b) => return printByte(b);
		Bool(b) => return printBool(b);
		Void(v) => return printVoid(v);
	}
}
