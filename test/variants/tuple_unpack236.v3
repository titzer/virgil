//@execute 0=134217729; 1=134217729; 2=129
def main(a: int) -> int {
	def p = u64.pack<(u28,u28,byte)>((0b1000000000000000000000000001,0b1000000000000000000000000001,byte.view(0b10000001)));
	def u: (u28,u28,byte) = u64.unpack<(u28,u28,byte)>(p);
	match (a) {	
		0 => return u.0; 
		1 => return u.1; 
		2 => return u.2;
		_ => return 42; 
	}
}
