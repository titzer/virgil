//@execute 0=1; 1=1; 2=1; 3=1; 4=1; 5=47
type T {
	case A;
	case B;
}

type X(v1: T.B, v2: T.B) #unboxed;

def isB(t: T) -> int {
	return if(T.B.?(t), 1, 0);
}

def main(a: int) -> int {
	var tb: T.B;
	var x1: X;
	var x2 = X(tb, tb);
	match (a) {
		0 => return isB(tb);
		1 => return isB(x1.v1);
		2 => return isB(x1.v2);
		3 => return isB(x2.v1);
		4 => return isB(x2.v2);
	}
	return 47;
}
