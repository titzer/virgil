//@execute 0=1; 1=1; 2=2; 3=3; 4=1; 5=4; 6=1; 7=-5; 8=1; 9=1; 10=-6; 11=1
enum E {
    X, Y, Z
}
type C1(a: E.set, b: u15) #unboxed #packed{}
type C2(a: E.set, b: i26) #unboxed #packed{}
type A #unboxed #packed {
    case One(a: u13, b: C1) #unboxed #packed;
    case Two(a: C2, c: E.set) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(1, C1(E.X|E.Z, 2)), 
    A.One(3, C1(E.Y, 4)), 
    A.Two(C2(E.Z|E.X, -5), E.X|E.Y), 
    A.Two(C2(E.Y, -6), E.Z|E.X)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return int.view<bool>(A.One.!(arr[0]).b.a == (E.X|E.Z));
        2 => return A.One.!(arr[0]).b.b;

        3 => return A.One.!(arr[1]).a;
        4 => return int.view<bool>(A.One.!(arr[1]).b.a == E.Y);
        5 => return A.One.!(arr[1]).b.b;

        6 => return int.view<bool>(A.Two.!(arr[2]).a.a == (E.X|E.Z));
        7 => return A.Two.!(arr[2]).a.b;
        8 => return int.view<bool>(A.Two.!(arr[2]).c == (E.X|E.Y));

        9 => return int.view<bool>(A.Two.!(arr[3]).a.a == E.Y);
        10 => return A.Two.!(arr[3]).a.b;
        11 => return int.view<bool>(A.Two.!(arr[3]).c == (E.X|E.Z));
        _ => return 42;
    }
}