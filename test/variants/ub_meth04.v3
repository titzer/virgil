//@execute 0=0; 1=36; 2=447; 3=559; 4=0; 5=!BoundsCheckException
type X {
	case A(v: int)			{ def m() -> int { return v; } }
	case B(v: long) #unboxed	{ def m() -> int { return int.view(v); } }

	def m() -> int;
}

var d: X.A;
var e: X.B;
var vals = [d, X.A(36), X.B(447L), X.B(559L), e];

def main(a: int) -> int {
	return vals[a].m();
}
