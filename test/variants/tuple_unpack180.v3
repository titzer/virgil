//@execute 0=32769; 1=4097; 2=1
enum E { A, B, C }
def main(a: int) -> int {
	def p = u32.pack<(u16,u13,E)>((0b1000000000000001,0b1000000000001,E.A));
	def u: (u16,u13,E) = u32.unpack<(u16,u13,E)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1;
		2 => return if(u.2==E.A, 1, 0);
		_ => return 42;
	}
}
