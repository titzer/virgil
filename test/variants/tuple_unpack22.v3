//@execute 0=32769; 1=0; 2=16385
def main(a: int) -> int {
	def p = int.pack<(u16,bool,u15)>((0b1000000000000001,false,0b100000000000001));
	def u: (u16,bool,u15) = int.unpack<(u16,bool,u15)>(p);
	match (a) {
		0 => return u.0; 
		1 => return if(u.1, 1, 0);
		2 => return u.2;
		_ => return 42;
	}
}
