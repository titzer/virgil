//@execute 0=0; 1=33; 2=444; 3=555; 4=!BoundsCheckException
type X #unboxed {
	case A(v: int)	{ def m() -> int { return v; } }
	case B(v: long)	{ def m() -> int { return int.view(v); } }

	def m() -> int;
}

var vals = [X.A(0), X.A(33), X.B(444L), X.B(555L)];

def main(a: int) -> int {
	return vals[a].m();
}
