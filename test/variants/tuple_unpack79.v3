//@execute 0=129; 1=32769; 2=1048577
def main(a: int) -> int {
	def p = i45.pack<(byte,u16,u21)>((byte.view(0b10000001),0b1000000000000001,0b100000000000000000001));
	def u: (byte,u16,u21) = i45.unpack<(byte,u16,u21)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1; 
		2 => return u.2; 
		_ => return 42; 
	}
}
