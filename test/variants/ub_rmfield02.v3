//@execute 0=114
type X #unboxed {
	case Zero;
	case One(x: int);
	case Two(x: int, y: int);
	case Three(x: int, y: int, z: int);
}

def inputs = [
	X.Zero,
	X.One(11),
	X.One(12),
	X.Two(13, 14),
	X.Two(15, 16),
	X.Three(17, 18, 19),
	X.Three(20, 21, 22)
];

def main() -> int {
	var x: X, sum = 0;
	for (i < inputs.length) {
		match (x) {
			Zero => sum += -1;
			One(x) => sum += x;
			Two(x, y) => sum += x + y;
			Three(x, y, z) => sum += x + y; // z unused
		}
		x = get(i);
	}
	return sum;
}

def get(i: int) -> X {
	return inputs[i];
}