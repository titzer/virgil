//@execute 0=1; 1=32769; 2=1
enum E { A, B, C }
def main(a: int) -> int {
	def p = i64.pack<(E,u16,u45)>((E.A,0b1000000000000001,0b100000000000000000000000000000000000000000001));
	def u: (E,u16,u45) = i64.unpack<(E,u16,u45)>(p);
	match (a) {
		0 => return if(u.0==E.A, 1, 0); 
		1 => return u.1; 
		2 => return if(u.2==u45.view(0b100000000000000000000000000000000000000000001), 1, 0); 
		_ => return 42; 
	}
}
