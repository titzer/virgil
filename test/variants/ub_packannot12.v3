//@execute 0=5;1=14
type A00 #unboxed #packed {
	case X(a: u4) #packing 0b_?aaaa {
		def f() -> u4 { return a; }
	}
	case Y(b: u4) #packing 0b_?bbbb {
		def f() -> u4 { return b * 2u4; }
	}

	def f() -> u4;
}

def arr = [
	A00.X(5u4),
	A00.Y(7u4)
];

def main(a: int) -> int {
	return int.view(arr[a].f());
}