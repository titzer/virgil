//@execute 0=6; 1=15; 2=15; 3=19
type A #unboxed #packed {
    case One(a: u8, b: u16, c: u7) #unboxed #packed;
    case Two(a: u16, b: u15) #unboxed #packed;
}
def arr = [
    A.One(1, 2, 3), 
    A.One(4, 5, 6), 
    A.Two(7, 8), 
    A.Two(9, 10)
];

def main(a: int) -> int {
    match(arr[a]) {
        One(a, b, c) => return a + b + c;
        Two(a, b) => return a + b;
    }
}