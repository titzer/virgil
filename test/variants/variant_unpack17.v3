//@execute 0=1; 1=3; 2=3; 3=3; 4=1; 5=3; 6=3; 7=3; 8=1
type C1 (i1: int, i2: u2, i3: u2){ }
type C2 (i1: u2, i2: int, i3: u2){ }
type C3 (i1: u2, i2: u2, i3: int){ }
def main(a: int) -> int {
    def c1: C1 = C1(0b10000000000000000000000000000001, 0b11, 0b11);
    def c2: C2 = C2(0b11, 0b10000000000000000000000000000001, 0b11);
    def c3: C3 = C3(0b11, 0b11, 0b10000000000000000000000000000001);

    def u1 = long.unpack<C1>(long.pack<C1>(c1));
    def u2 = long.unpack<C2>(long.pack<C2>(c2));
    def u3 = long.unpack<C3>(long.pack<C3>(c3));

    match (a) {
        0 => return if(u1.i1==-2147483647, 1, 0); 
        1 => return u1.i2;
        2 => return u1.i3;
        3 => return u2.i1; 
        4 => return if(u2.i2==-2147483647, 1, 0);
        5 => return u2.i3;
        6 => return u3.i1; 
        7 => return u3.i2;
        8 => return if(u3.i3==-2147483647, 1, 0);
        _ => return 42;
    }
}