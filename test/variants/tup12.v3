//@execute 0=42; -42=0; 42=84
type T {
	case A(x: int, y: int);
	case B(x: bool, y: bool);
}

def main(a: int) -> int {
	var va1 = T.A(a, a + 1);
	var va2 = T.A((a, a + 1));

	if (!eq(va1, va2)) return -77;

	var n = a < 0;
	var vb1 = T.B(n, !n);
	var vb2 = T.B((n, !n));

	if (!eq(vb1, vb2)) return -77;

	return 42 + a;
}

def eq(a: T, b: T) -> bool {
	return a == b;
}
