//@execute 0=1; 1=0; 2=2; 3=3; 4=1; 5=4; 6=2; 7=-5; 8=0; 9=1; 10=-6; 11=2
enum E {
    X, Y, Z
}
type A #unboxed #packed {
    case One(a: u13, b: E, c: u15) #unboxed #packed;
    case Two(a: E, b: i26, c: E) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(1, E.X, 2), 
    A.One(3, E.Y, 4), 
    A.Two(E.Z, -5, E.X), 
    A.Two(E.Y, -6, E.Z)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return A.One.!(arr[0]).b.tag;
        2 => return A.One.!(arr[0]).c;

        3 => return A.One.!(arr[1]).a;
        4 => return A.One.!(arr[1]).b.tag;
        5 => return A.One.!(arr[1]).c;

        6 => return A.Two.!(arr[2]).a.tag;
        7 => return A.Two.!(arr[2]).b;
        8 => return A.Two.!(arr[2]).c.tag;

        9 => return A.Two.!(arr[3]).a.tag;
        10 => return A.Two.!(arr[3]).b;
        11 => return A.Two.!(arr[3]).c.tag;
        _ => return 42;
    }
}