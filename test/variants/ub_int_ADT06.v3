//@execute 0=21; 1=37; 2=10; 3=11; 4=13; 5=18
type N #unboxed #packed {
    case N1(a: u4, b: u11) #unboxed #packed;
    case N2(a: u5, b: u10) #unboxed #packed;
}

type A #unboxed #packed {
    case One(a: N.N1, b: N.N1) #unboxed #packed;
    case Two(a: u30) #unboxed #packed;
    case Three(a: u15, b: N.N2) #unboxed #packed;
}

def arr = [
    A.One(N.N1(1, 5), N.N1(4, 11)), 
    A.One(N.N1(4, 11), N.N1(7, 15)), 
    A.Two(10),
    A.Two(11),
    A.Three(7, N.N2(1, 5)), 
    A.Three(9, N.N2(2, 7))
];

def main(a: int) -> int {
    match(arr[a]) {
        One(a, b) => return a.a + a.b + b.a + b.b;
        Two(a) => return a;
        Three(a, b) => return a + b.a + b.b;
    }
}