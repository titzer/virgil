//@execute 0=32769; 1=32769; 2=1
def main(a: int) -> int {
	def p = i64.pack<(u16,u16,float)>((0b1000000000000001,0b1000000000000001,1F));
	def u: (u16,u16,float) = i64.unpack<(u16,u16,float)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1; 
		2 => return if(u.2==1F, 1, 0); 
		_ => return 42; 
	}
}
