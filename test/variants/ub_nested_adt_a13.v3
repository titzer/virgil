//@execute 0=1; 1=2; 2=10; 3=1; 4=3; 5=11; 6=10; 7=2; 8=1; 9=20; 10=3; 11=1; 12=1; 13=1
type C1(a: u4, b: u23)#unboxed #packed{}
type C2(a: u24, b: u3)#unboxed #packed{}
type A #unboxed #packed {
    case One(a: float, b: C1) #unboxed #packed;
    case Two(a: C2, c: float) #unboxed #packed;
    case Three(a: float) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(1F, C1(2, 10)), 
    A.One(4F, C1(3, 11)), 
    A.Two(C2(10, 2), 7F), 
    A.Two(C2(20, 3), 10F), 
    A.Three(11F), 
    A.Three(12F)
];

def main(a: int) -> int {
    match(a) {
        0 => return if(A.One.!(arr[0]).a == 1F, 1, 0);
        1 => return A.One.!(arr[0]).b.a;
        2 => return A.One.!(arr[0]).b.b;

        3 => return if(A.One.!(arr[1]).a == 4F, 1, 0);
        4 => return A.One.!(arr[1]).b.a;
        5 => return A.One.!(arr[1]).b.b;

        6 => return A.Two.!(arr[2]).a.a;
        7 => return A.Two.!(arr[2]).a.b;
        8 => return if(A.Two.!(arr[2]).c == 7F, 1, 0);

        9 => return A.Two.!(arr[3]).a.a;
        10 => return A.Two.!(arr[3]).a.b;
        11 => return if(A.Two.!(arr[3]).c == 10F, 1, 0);

        12 => return if(A.Three.!(arr[4]).a == 11F, 1, 0);

        13 => return if(A.Three.!(arr[5]).a == 12F, 1, 0);
        _ => return 42;
    }
}