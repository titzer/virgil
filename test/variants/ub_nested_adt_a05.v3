//@execute 0=1; 1=2; 2=1; 3=4; 4=5; 5=0; 6=7; 7=1; 8=9; 9=0
type C(a: u16, b: bool)#unboxed #packed{}
type A #unboxed #packed {
    case One(a: u8, b: C) #unboxed #packed;
    case Two(a: u16, b: bool) #unboxed #packed;
}
def arr: Array<A> = [
    A.One(1, C(2, true)), 
    A.One(4, C(5, false)), 
    A.Two(7, true), 
    A.Two(9, false)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return A.One.!(arr[0]).b.a;
        2 => return int.view<bool>(A.One.!(arr[0]).b.b);

        3 => return A.One.!(arr[1]).a;
        4 => return A.One.!(arr[1]).b.a;
        5 => return int.view<bool>(A.One.!(arr[1]).b.b);

        6 => return A.Two.!(arr[2]).a;
        7 => return int.view<bool>(A.Two.!(arr[2]).b);

        8 => return A.Two.!(arr[3]).a;
        9 => return int.view<bool>(A.Two.!(arr[3]).b);
        _ => return 42;
    }
}