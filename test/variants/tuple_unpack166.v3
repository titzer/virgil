//@execute 0=0; 1=32769; 2=16385
def main(a: int) -> int {
	def p = u32.pack<(bool,u16,u15)>((false,0b1000000000000001,0b100000000000001));
	def u: (bool,u16,u15) = u32.unpack<(bool,u16,u15)>(p);
	match (a) {
		0 => return if(u.0, 1, 0); 
		1 => return u.1; 
		2 => return u.2; 
		_ => return 42;
	}
}
