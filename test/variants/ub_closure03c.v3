//@execute 0=1; 1=21; 2=727379970; 3=5
type T #unboxed {
	case A(f: void -> int);
	case C(x: long);
}

var ts = [T.A(f1), T.A(f2), T.C(2L), T.C(1_000_000_000_005L)];

def main(a: int) -> int {
	match (ts[a]) {
		A(f) => return f();
		C(f) => return int.view(f - 1_000_000_000_000L);
	}
}

def f1() -> int { return 1; }
def f2() -> int { return 21; }
