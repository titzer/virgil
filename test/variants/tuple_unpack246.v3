enum E { A, B, C }
def main() -> bool {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = u64.pack<(E.set,u16,u45)>((e5,0b1000000000000001,0b100000000000000000000000000000000000000000001));
	def u: (E.set,u16,u45) = u64.unpack<(E.set,u16,u45)>(p);
	return p == u64.pack<(E.set,u16,u45)>(u);
}
