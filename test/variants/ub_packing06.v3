//@execute 0=true;1=true;2=true;3=true
type A00 #unboxed #packed {
	case X(a: double) {
		def f() -> double { return a; }
	}
	case Y(c: float, d: float) {
		def f() -> double { return double.!(c) + double.!(d); }
	}

	def f() -> double;
}

def arr = [
	A00.X(0.0d),
	A00.X(5.0d),
	A00.Y(0.0f, 1.0f),
	A00.Y(1.0f, 2.0f)
];

def ans = [
	0.0d,
	5.0d,
	double.!(0.0f) + double.!(1.0f),
	double.!(1.0f) + double.!(2.0f)
];

def main(a: int) -> bool {
	return arr[a].f() == ans[a];
}