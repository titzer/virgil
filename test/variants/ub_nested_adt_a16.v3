//@execute 0=1; 1=1; 2=1; 3=3; 4=0; 5=1; 6=1; 7=6; 8=1; 9=0; 10=8; 11=1
type C1(a: bool, b: float) #unboxed #packed {}
type C2(a: bool, b: u30) #unboxed #packed {}
type A #unboxed #packed {
    case One(a: u30, c: C1) #unboxed #packed;
    case Two(a: C2, c: float) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(1, C1(true, 1F)), 
    A.One(3, C1(false, 2F)), 
    A.Two(C2(true, 6), 1F), 
    A.Two(C2(false, 8), 2F)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return int.view<bool>(A.One.!(arr[0]).c.a);
        2 => return if(A.One.!(arr[0]).c.b == 1F, 1, 0);

        3 => return A.One.!(arr[1]).a;
        4 => return int.view<bool>(A.One.!(arr[1]).c.a);
        5 => return if(A.One.!(arr[1]).c.b == 2F, 1, 0);

        6 => return int.view<bool>(A.Two.!(arr[2]).a.a);
        7 => return A.Two.!(arr[2]).a.b;
        8 => return if(A.Two.!(arr[2]).c == 1F, 1, 0);

        9 => return int.view<bool>(A.Two.!(arr[3]).a.a);
        10 => return A.Two.!(arr[3]).a.b;
        11 => return if(A.Two.!(arr[3]).c == 2F, 1, 0);
        _ => return 42;
    }
}