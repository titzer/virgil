def main() -> bool {
	def p = u64.pack<((byte,u8,u8),u20,u20)>(((byte.view(0b10000001),0b10000001,0b10000001),0b10000000000000000001,0b10000000000000000001));
	def u: ((byte,u8,u8),u20,u20) = u64.unpack<((byte,u8,u8),u20,u20)>(p);
	return p == u64.pack<((byte,u8,u8),u20,u20)>(u);
