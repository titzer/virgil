enum E { A, B, C }
def main() -> bool {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = i32.pack<(u16,u13,E.set)>((0b1000000000000001,0b1000000000001,e5));
	def u: (u16,u13,E.set) = i32.unpack<(u16,u13,E.set)>(p);
	return p == i32.pack<(u16,u13,E.set)>(u);
}
