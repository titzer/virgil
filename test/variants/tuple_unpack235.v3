//@execute 0=134217729; 1=129; 2=134217729
def main(a: int) -> int {
	def p = u64.pack<(u28,byte,u28)>((0b1000000000000000000000000001,byte.view(0b10000001),0b1000000000000000000000000001));
	def u: (u28,byte,u28) = u64.unpack<(u28,byte,u28)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1; 
		2 => return u.2; 
		_ => return 42; 
	}
}
