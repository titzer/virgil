//@execute =true;
enum E {
    A, B, C, D
}

def main() -> bool {
    // Test the same on both halves
    def input1: E.set = E.A;
    def expected1: int = 0b00010001;
    if(expected1 != int.pack<(E.set, E.set)>((input1, input1))) { def x = 1/0; };

    def input6: E.set = E.A | E.C;
    def expected6: int = 0b01010101;
    if(expected6 != int.pack<(E.set, E.set)>((input6, input6))) { def x = 1/0; };

    // Test different elements
    def input26a: E.set = E.A | E.C;
    def input26b: E.set = E.B | E.D;
    def expected26: int = 0b10100101;
    if(expected26 != int.pack<(E.set, E.set)>((input26b, input26a))) { def x = 1/0; };

    def input29a: E.set = E.A | E.B;
    def input29b: E.set = E.A | E.C;
    def expected29: int = 0b01010011;
    if(expected29 != int.pack<(E.set, E.set)>((input29b, input29a))) { def x = 1/0; };

    return true;
}