//@execute 0=1; 1=2; 2=1; 3=4; 4=5; 5=1; 6=7; 7=1; 8=9; 9=1
type A #unboxed #packed {
    case One(a: u8, b: u16, c: float) #unboxed #packed;
    case Two(a: u16, b: float) #unboxed #packed;
}
def arr: Array<A> = [
    A.One(1, 2, 3F), 
    A.One(4, 5, 1F), 
    A.Two(7, 7F), 
    A.Two(9, 9F)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return A.One.!(arr[0]).b;
        2 => return if(A.One.!(arr[0]).c == 3F, 1, 0);

        3 => return A.One.!(arr[1]).a;
        4 => return A.One.!(arr[1]).b;
        5 => return if(A.One.!(arr[1]).c == 1F, 1, 0);

        6 => return A.Two.!(arr[2]).a;
        7 => return if(A.Two.!(arr[2]).b == 7F, 1, 0);

        8 => return A.Two.!(arr[3]).a;
        9 => return if(A.Two.!(arr[3]).b == 9F, 1, 0);
        _ => return 42;
    }
}