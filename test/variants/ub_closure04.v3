//@execute 0=false; 1=true; 2=false; 3=false; 4=false; 5=true
type T #unboxed {
	case A(x: bool, y: bool, z: bool, w: bool);
	case B(f: (int, int) -> bool);
}

var ts = [
	T.A(false, false, false, false),
	T.A( true,  true,  true , true),
	T.A(false, false,  true,  true),
	T.A( true,  true, false, false),
	T.B(int.==),
	T.B(int.!=)
];

def main(a: int) -> bool {
	match (ts[a]) {
		A(x, y, z, w) => return x & y & z & w;
		B(f) => return f(3, 4);
	}
}
