//@execute 0=1; 1=0; 2=1
def main(a: int) -> int {
	def p = i64.pack<(u32,bool,u31)>((0b10000000000000000000000000000001,false,0b1000000000000000000000000000001));
	def u: (u32,bool,u31) = i64.unpack<(u32,bool,u31)>(p);
	match (a) {
		0 => return if(u.0==u32.view(0b10000000000000000000000000000001), 1, 0); 
		1 => return if(u.1, 1, 0); 
		2 => return if(u.2==u31.view(0b1000000000000000000000000000001), 1, 0); 
		_ => return 42;
	}
}
