//@execute 0=0; 1=3; 2=7
type T #unboxed {
	case A(f: void -> int);
	case B(x: int);
	case C(x: long);
}


def main(a: int) -> int {
	var ts = [T.A(f1(a, _)), T.B(2 + a), T.C(1_000_000_000_005L + a)];
	match (ts[a]) {
		A(f) => return f();
		B(x) => return x;
		C(f) => return int.view(f - 1_000_000_000_000L);
	}
}

def f1(x: int, v: void) -> int { return x; }
