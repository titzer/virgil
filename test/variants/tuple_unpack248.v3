//@execute 0=32769; 1=1; 2=1
enum E { A, B, C }
def main(a: int) -> int {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = u64.pack<(u16,u45,E.set)>((0b1000000000000001,0b100000000000000000000000000000000000000000001,e5));
	def u: (u16,u45,E.set) = u64.unpack<(u16,u45,E.set)>(p);
	match (a) {	
		0 => return u.0; 
		1 => return if(u.1==u45.view(0b100000000000000000000000000000000000000000001), 1, 0);
		2 => return if(u.2==e5, 1, 0); 
		_ => return 42; 
	}
}
