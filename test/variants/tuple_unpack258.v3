//@execute 0=1; 1=32769; 2=32769
def main(a: int) -> int {
	def p = u64.pack<(float,u16,u16)>((1F,0b1000000000000001,0b1000000000000001));
	def u: (float,u16,u16) = u64.unpack<(float,u16,u16)>(p);
	match(a) {
		0 => return if(u.0==1F, 1, 0);
		1 => return u.1;
		2 => return u.2;
		_ => return 42; 
	}
}
