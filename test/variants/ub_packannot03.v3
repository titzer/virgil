//@execute 0=6;1=12;2=-1;3=-1
type T #packed #unboxed {
	case A(x: int, y: int, z: int) 	#packing (x, #concat(y, z)) { def f() -> int { return x + y + z; } }
	case B(x: u64, y: int) 		#packing (x, y) { def f() -> int { return int.view(x) - y; } }

	def f() -> int;
}

def arr = [
	T.A(1, 2, 3),
	T.A(3, 4, 5),
	T.B(5, 6),
	T.B(7, 8)
];

def main(a: int) -> int {
	return arr[a].f();
}