//@execute 0=99; 1=727379971; 2=7
type T #unboxed {
	case A(f: void -> int);
	case C(x: long);
}


def main(a: int) -> int {
	var ts = make(a);
	match (ts[a]) {
		A(f) => return f();
		C(f) => return int.view(f - 1_000_000_000_000L);
	}
}
def make(a: int) -> Array<T> {
	return [T.A(f1), T.C(2 + a), T.C(1_000_000_000_005L + a)];
}

def f1() -> int { return 99; }
