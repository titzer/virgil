//@execute 0=21; 1=37; 2=13; 3=18; 4=10; 5=11
type A #unboxed #packed {
    case One(a: (u4, u8, u3), b: (u4, u8, u3)) #unboxed #packed;
    case Two(a: u15, b: (u5, u5, u5)) #unboxed #packed;
    case Three(a: u30) #unboxed #packed;
}
def arr = [
    A.One((1, 2, 3), (4, 5, 6)), 
    A.One((4, 5, 6), (7, 8, 7)), 
    A.Two(7, (1, 2, 3)), 
    A.Two(9, (2, 3, 4)),
    A.Three(10),
    A.Three(11)
];

def main(a: int) -> int {
    match(arr[a]) {
        One(a, b) => return a.0 + a.1 + a.2 + b.0 + b.1 + b.2;
        Two(a, b) => return a + b.0 + b.1 + b.2;
        Three(a) => return a;
    }
}