def main(a: int) -> int {
	def p = i45.pack<(u22,bool,u22)>((0b1000000000000000000001,false,0b1000000000000000000001));
	def u: (u22,bool,u22) = i45.unpack<(u22,bool,u22)>(p);
	match (a) {
		0 => return u.0;
		1 => return if(u.1, 1, 0); 
		2 => return u.2; 
		_ => return 42; 
	}
}
