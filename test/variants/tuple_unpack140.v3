//@execute 0=32769; 1=1; 2=32769
def main(a: int) -> int {
	def p = i64.pack<(u16,float,u16)>((0b1000000000000001,1F,0b1000000000000001));
	def u: (u16,float,u16) = i64.unpack<(u16,float,u16)>(p);
	match (a) {
		0 => return u.0; 
		1 => return if(u.1==1F, 1, 0); 
		2 => return u.2; 
		_ => return 42; 
	}
}
