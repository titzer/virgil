//@execute 0=32769; 1=1048577; 2=129
def main(a: int) -> int {
	def p = u45.pack<(u16,u21,byte)>((0b1000000000000001,0b100000000000000000001,byte.view(0b10000001)));
	def u: (u16,u21,byte) = u45.unpack<(u16,u21,byte)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1; 
		2 => return u.2; 
		_ => return 42; 
	}
}
