//@execute 0=32769; 1=1; 2=4097
enum E { A, B, C }
def main(a: int) -> int {
	def p = i32.pack<(u16,E,u13)>((0b1000000000000001,E.A,0b1000000000001));
	def u: (u16,E,u13) = i32.unpack<(u16,E,u13)>(p);
	match (a) {
		0 => return u.0; 
		1 => return if(u.1==E.A, 1, 0); 
		2 => return u.2;
		_ => return 42;
	}
}
