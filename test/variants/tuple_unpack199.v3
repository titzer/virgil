//@execute 0=32769; 1=129; 2=1048577
def main(a: int) -> int {
	def p = u45.pack<(u16,byte,u21)>((0b1000000000000001,byte.view(0b10000001),0b100000000000000000001));
	def u: (u16,byte,u21) = u45.unpack<(u16,byte,u21)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1; 
		2 => return u.2; 
		_ => return 42; 
	}
}
