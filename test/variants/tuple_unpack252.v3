//@execute 0=-2147483647; 1=32769; 2=32769
def main(a: int) -> int {
	def p = u64.pack<(int,u16,u16)>((0b10000000000000000000000000000001,0b1000000000000001,0b1000000000000001));
	def u: (int,u16,u16) = u64.unpack<(int,u16,u16)>(p);
	match(a) {
		0 => return u.0;
		1 => return u.1;
		2 => return u.2;
		_ => return 42; 
	}
}
