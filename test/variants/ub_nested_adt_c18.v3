//@execute 0=1; 1=-2; 2=1; 3=1; 4=3; 5=-4; 6=0; 7=1; 8=-5; 9=1; 10=1; 11=6; 12=-7; 13=1; 14=0; 15=8
type C1(a: u15, b: i14) #unboxed #packed {}
type C2(a: bool, b: float) #unboxed #packed {}
type C3(a: i14, b: float) #unboxed #packed {}
type C4(a: bool, b: u15) #unboxed #packed {}
type A #unboxed #packed {
    case One(a: C1, b: C2) #unboxed #packed;
    case Two(a: C3, b: C4) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(C1(1, -2), C2(true, 1F)), 
    A.One(C1(3, -4), C2(false, 2F)), 
    A.Two(C3(-5, 1F), C4(true, 6)), 
    A.Two(C3(-7, 2F), C4(false, 8))
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a.a;
        1 => return A.One.!(arr[0]).a.b;
        2 => return int.view<bool>(A.One.!(arr[0]).b.a);
        3 => return if(A.One.!(arr[0]).b.b == 1F, 1, 0);

        4 => return A.One.!(arr[1]).a.a;
        5 => return A.One.!(arr[1]).a.b;
        6 => return int.view<bool>(A.One.!(arr[1]).b.a);
        7 => return if(A.One.!(arr[1]).b.b == 2F, 1, 0);

        8 => return A.Two.!(arr[2]).a.a;
        9 => return if(A.Two.!(arr[2]).a.b == 1F, 1, 0);
        10 => return int.view<bool>(A.Two.!(arr[2]).b.a);
        11 => return A.Two.!(arr[2]).b.b;

        12 => return A.Two.!(arr[3]).a.a;
        13 => return if(A.Two.!(arr[3]).a.b == 2F, 1, 0);
        14 => return int.view<bool>(A.Two.!(arr[3]).b.a);
        15 => return A.Two.!(arr[3]).b.b;
        _ => return 42;
    }
}