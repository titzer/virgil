//@execute 0=1; 1=1; 2=1; 3=1; 4=1
enum E { A, B, C }
def main(a: int) -> int {
	def p = u7.pack<(u1,(u1,u1,E),u1)>((0b1,(0b1,0b1,E.A),0b1));
	def u: (u1,(u1,u1,E),u1) = u7.unpack<(u1,(u1,u1,E),u1)>(p);
	match (a) {
		0 => return u.0; 
		1 => return u.1.0;
		2 => return u.1.1;
		3 => return if(u.1.2==E.A, 1, 0);
		4 => return u.2; 
		_ => return 42; 
	}
}
