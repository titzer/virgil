//@execute 0=32769; 1=-2147483647; 2=32769
def main(a: int) -> int {
	def p = u64.pack<(u16,int,u16)>((0b1000000000000001,0b10000000000000000000000000000001,0b1000000000000001));
	def u: (u16,int,u16) = u64.unpack<(u16,int,u16)>(p);
	match(a) {
		0 => return u.0;
		1 => return u.1;
		2 => return u.2;
		_ => return 42; 
	}
}
