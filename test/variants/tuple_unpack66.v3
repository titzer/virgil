//@execute 0=32769; 1=1; 2=4097
enum E { A, B, C }
def main(a: int) -> int {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = i32.pack<(u16,E.set,u13)>((0b1000000000000001,e5,0b1000000000001));
	def u: (u16,E.set,u13) = i32.unpack<(u16,E.set,u13)>(p);
	match (a) {
		0 => return u.0; 
		1 => return if(u.1 == e5, 1, 0); 
		2 => return u.2; 
		_ => return 42; 
	}
}
