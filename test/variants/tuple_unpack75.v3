def main(a: int) -> int {
	def p = i45.pack<(u22,u22,bool)>((0b1000000000000000000001,0b1000000000000000000001,false));
	def u: (u22,u22,bool) = i45.unpack<(u22,u22,bool)>(p);
	match (a) {
		0 => return u.0;
		1 => return u.1; 
		2 => return if(u.2, 1, 0); 
		_ => return 42; 
	}
}
