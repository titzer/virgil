//@execute 0=32769; 1=1; 2=1
enum E { A, B, C }
def main(a: int) -> int {
	def p = i64.pack<(u16,E,u45)>((0b1000000000000001,E.A,0b100000000000000000000000000000000000000000001));
	def u: (u16,E,u45) = i64.unpack<(u16,E,u45)>(p);
	match (a) {
		0 => return u.0; 
		1 => return if(u.1==E.A, 1, 0); 
		2 => return if(u.2==u45.view(0b100000000000000000000000000000000000000000001), 1, 0); 
		_ => return 42; 
	}
}
