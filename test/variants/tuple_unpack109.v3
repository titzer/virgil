//@execute 0=0; 1=1; 2=1
def main(a: int) -> int {
	def p = i64.pack<(bool,u32,u31)>((false,0b10000000000000000000000000000001,0b1000000000000000000000000000001));
	def u: (bool,u32,u31) = i64.unpack<(bool,u32,u31)>(p);
	match (a) {
		0 => return if(u.0, 1, 0); 
		1 => return if(u.1==u32.view(0b10000000000000000000000000000001), 1, 0); 
		2 => return if(u.2==u31.view(0b1000000000000000000000000000001), 1, 0); 
		_ => return 42;
	}
}
