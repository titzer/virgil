//@execute 0=1; 1=32769; 2=513; 3=65537; 4=131073
enum E { A, B, C }
def main(a: int) -> int {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = i64.pack<((E.set,u16,u10),u17,u18)>(((e5,0b1000000000000001,0b1000000001),0b10000000000000001,0b100000000000000001));
	def u: ((E.set,u16,u10),u17,u18) = i64.unpack<((E.set,u16,u10),u17,u18)>(p);
	match (a) {
		0 => return if(u.0.0==e5, 1, 0);
		1 => return u.0.1; 
		2 => return u.0.2; 
		3 => return u.1; 
		4 => return u.2; 
		_ => return 42; 
	}
}
