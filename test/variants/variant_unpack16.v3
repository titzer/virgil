//@execute 0=3; 1=-2147483647; 2=3
type C (i1: u2, i2: int, i3: u2){ }
def main(a:int) -> int {
    def c: C = C(0b11, 0b10000000000000000000000000000001, 0b11);
    def u = long.unpack<C>(long.pack<C>(c));
    match (a) {
        0 => return u.i1;
        1 => return u.i2; 
        2 => return u.i3;
        _ => return 42; 
    }
}