//@execute 0=1; 1=-2; 2=1; 3=3; 4=-4; 5=0; 6=5; 7=1; 8=-6; 9=7; 10=0; 11=-8; 12=1; 13=9; 14=-10; 15=0; 16=11; 17=-12
type A #unboxed #packed {
    case One(a: (u17, i12), c: bool) #unboxed #packed;
    case Two(a: (u17, bool), c: i12) #unboxed #packed;
    case Three(a: (bool, u17), c: i12) #unboxed #packed;
}

def arr: Array<A> = [
    A.One((1, -2), true), 
    A.One((3, -4), false), 
    A.Two((5, true), -6), 
    A.Two((7, false), -8), 
    A.Three((true, 9), -10), 
    A.Three((false, 11), -12)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a.0;
        1 => return A.One.!(arr[0]).a.1;
        2 => return int.view<bool>(A.One.!(arr[0]).c);

        3 => return A.One.!(arr[1]).a.0;
        4 => return A.One.!(arr[1]).a.1;
        5 => return int.view<bool>(A.One.!(arr[1]).c);

        6 => return A.Two.!(arr[2]).a.0;
        7 => return int.view<bool>(A.Two.!(arr[2]).a.1);
        8 => return A.Two.!(arr[2]).c;

        9 => return A.Two.!(arr[3]).a.0;
        10 => return int.view<bool>(A.Two.!(arr[3]).a.1);
        11 => return A.Two.!(arr[3]).c;

        12 => return int.view<bool>(A.Three.!(arr[4]).a.0);
        13 => return A.Three.!(arr[4]).a.1;
        14 => return A.Three.!(arr[4]).c;

        15 => return int.view<bool>(A.Three.!(arr[5]).a.0);
        16 => return A.Three.!(arr[5]).a.1;
        17 => return A.Three.!(arr[5]).c;

        _ => return 42;
    }
}