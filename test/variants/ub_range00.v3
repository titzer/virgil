//@execute 0=0;1=4;2=12;3=23;4=10;5=0;6=!NullCheckException
type T #unboxed {
	case A(x: Range<byte>) { def f() -> int { return x.length; } }
	case B(x: int) { def f() -> int { return x; } }
	case C(x: string) { def f() -> int { return x.length * 2; } }

	def f() -> int;
}

def b = "hello world";

def arr = [
	T.A(b[0 ... 0]),
	T.A(b[1 ... 5]),
	T.B(12),
	T.B(23),
	T.C("hello"),
	T.C(""),
	T.C(null)
];

def main(a: int) -> int {
	return arr[a].f();
}