//@execute =true
enum E { A, B, C }
def main() -> bool {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = int.pack<(u6,(u8,E.set,u8),u7)>((0b100001,(0b10000001,e5,0b10000001),0b1000001));
	def u: (u6,(u8,E.set,u8),u7) = int.unpack<(u6,(u8,E.set,u8),u7)>(p);
	return p == int.pack<(u6,(u8,E.set,u8),u7)>(u);
}
