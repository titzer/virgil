//@execute 0=1; 1=-2; 2=1; 3=1; 4=3; 5=-4; 6=0; 7=1; 8=-5; 9=1; 10=1; 11=6; 12=-7; 13=1; 14=0; 15=8
type C1(a: u15, b: i14) #unboxed #packed {}
type C2(a: bool, b: u15) #unboxed #packed {}
type A #unboxed #packed {
    case One(a: C1, c: bool, d: float) #unboxed #packed;
    case Two(a: i14, b: float, c: C2) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(C1(1, -2), true, 1F), 
    A.One(C1(3, -4), false, 2F), 
    A.Two(-5, 1F, C2(true, 6)), 
    A.Two(-7, 2F, C2(false, 8))
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a.a;
        1 => return A.One.!(arr[0]).a.b;
        2 => return int.view<bool>(A.One.!(arr[0]).c);
        3 => return if(A.One.!(arr[0]).d == 1F, 1, 0);

        4 => return A.One.!(arr[1]).a.a;
        5 => return A.One.!(arr[1]).a.b;
        6 => return int.view<bool>(A.One.!(arr[1]).c);
        7 => return if(A.One.!(arr[1]).d == 2F, 1, 0);

        8 => return A.Two.!(arr[2]).a;
        9 => return if(A.Two.!(arr[2]).b == 1F, 1, 0);
        10 => return int.view<bool>(A.Two.!(arr[2]).c.a);
        11 => return A.Two.!(arr[2]).c.b;

        12 => return A.Two.!(arr[3]).a;
        13 => return if(A.Two.!(arr[3]).b == 2F, 1, 0);
        14 => return int.view<bool>(A.Two.!(arr[3]).c.a);
        15 => return A.Two.!(arr[3]).c.b;
        _ => return 42;
    }
}