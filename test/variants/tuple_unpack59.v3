//@execute 0=1; 1=32769; 2=4097
enum E { A, B, C }
def main(a: int) -> int {
	def p = i32.pack<(E,u16,u13)>((E.A,0b1000000000000001,0b1000000000001));
	def u: (E,u16,u13) = i32.unpack<(E,u16,u13)>(p);
	match (a) {
		0 => return if(u.0==E.A, 1, 0); 
		1 => return u.1; 
		2 => return u.2;
		_ => return 42;
	}
}
