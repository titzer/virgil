//@execute 0=1; 1=-2; 2=1; 3=1; 4=3; 5=-4; 6=0; 7=1; 8=-5; 9=1; 10=6; 11=1; 12=-7; 13=0; 14=8; 15=1
type C1(a: bool, b: float) #unboxed #packed {}
type C2(a: bool, b: u15) #unboxed #packed {}
type A #unboxed #packed {
    case One(a: u15, b: i14, c: C1) #unboxed #packed;
    case Two(a: i14, b: C2, d: float) #unboxed #packed;
}

def arr: Array<A> = [
    A.One(1, -2, C1(true, 1F)), 
    A.One(3, -4, C1(false, 2F)), 
    A.Two(-5, C2(true, 6), 1F), 
    A.Two(-7, C2(false, 8), 2F)
];

def main(a: int) -> int {
    match(a) {
        0 => return A.One.!(arr[0]).a;
        1 => return A.One.!(arr[0]).b;
        2 => return int.view<bool>(A.One.!(arr[0]).c.a);
        3 => return if(A.One.!(arr[0]).c.b == 1F, 1, 0);

        4 => return A.One.!(arr[1]).a;
        5 => return A.One.!(arr[1]).b;
        6 => return int.view<bool>(A.One.!(arr[1]).c.a);
        7 => return if(A.One.!(arr[1]).c.b == 2F, 1, 0);

        8 => return A.Two.!(arr[2]).a;
        9 => return int.view<bool>(A.Two.!(arr[2]).b.a);
        10 => return A.Two.!(arr[2]).b.b;
        11 => return if(A.Two.!(arr[2]).d == 1F, 1, 0);

        12 => return A.Two.!(arr[3]).a;
        13 => return int.view<bool>(A.Two.!(arr[3]).b.a);
        14 => return A.Two.!(arr[3]).b.b;
        15 => return if(A.Two.!(arr[3]).d == 2F, 1, 0);
        _ => return 42;
    }
}