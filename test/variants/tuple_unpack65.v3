//@execute 0=1; 1=32769; 2=4097
enum E { A, B, C }
def main(a: int) -> int {
	def e1: E.set = E.A;
	def e2: E.set = E.B;
	def e3: E.set = E.C;
	def e4: E.set = E.A|E.B;
	def e5: E.set = E.A|E.C;
	def e6: E.set = E.B|E.C;
	def e7: E.set = e4|E.C;
	def p = i32.pack<(E.set,u16,u13)>((e5,0b1000000000000001,0b1000000000001));
	def u: (E.set,u16,u13) = i32.unpack<(E.set,u16,u13)>(p);
	match (a) {
		0 => return if(u.0 == e5, 1, 0); 
		1 => return u.1; 
		2 => return u.2; 
		_ => return 42; 
	}
}
