//@execute 0=3; 1=0; 2=3
type C (i1: u2, i2: bool, i3: u2){ }
def main(a:int ) -> int {
    def c: C = C(0b11, false, 0b11);
    def u = int.unpack<C>(int.pack<C>(c));

    match (a) {
        0 => return u.i1; 
        1 => return if(u.i2, 1, 0); 
        2 => return u.i3; 
        _ => return 42; 
    }
}