def tuples = [(0, "str"), (1, "f")];

def main() {
	var sum = 0;
	for (e in tuples) {
		sum += e.0;
		sum += e.1.length;
	}
	System.puts("mixed: ");
	System.puts(if(CiRuntime.FEATURE_MIXED_ARRAYS, "true", "false"));
	System.ln();
	System.puts("sum: ");
	System.puti(sum);
	System.ln();
}
