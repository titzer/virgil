//@execute 0=1; 1=2; 2=4; 3=8; 4=16
struct L {
	=0;
}

def a = Array<byte>.new(4);
def b = Array<byte>.new(5);
def r1: Ref<L>;
def r2: Ref<L> = Ref<L>.at(a, 0);
def r3: Ref<L> = Ref<L>.at(a, 1);
def r4: Ref<L> = Ref<L>.at(b, 0);
def r5: Ref<L> = Ref<L>.at(b, 1);
def refs = [r1, r2, r3, r4, r5];

def main(a: int) -> int {
	var ref = refs[a];
	var result = 0;
	for (i < refs.length) {
		if (ref == refs[i]) result |= (1 << u5.view(i));
	}
	return result;
}
