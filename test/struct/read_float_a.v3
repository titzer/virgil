//@execute 0=22; 1=33; 2=!BoundsCheckException
struct F {
	+0	g:	float;
	=4;
}

def bytes: Array<byte> = [ 0x11, 0x22, 0x33, 0x44, 0x55 ];
def main(a: int) -> int {
	var r = Ref<F>.at(bytes, a);
	var g = r.g;
	if (g == float.view(0x44332211)) return 22;
	if (g == float.view(0x55443322)) return 33;
	return 44;
}
