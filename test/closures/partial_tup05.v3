//@execute (1,1,2)=13; (-1,1,2)=9; (3,7,9)=48; (-3,7,9)=36
component partial_tup05 {
	def main(a: int, b: int, c: int) -> int {
		var x = if(a >= 0, f(a, _, _), g(a, _));
		return x(b, c);
	}
	def f(a: int, b: int, c: int) -> int {
		return 2 * a - 3 * b + 7 * c;
	}
	def g(a: int, b: (int, int)) -> int {
		return 2 * a - 3 * b.0 + 7 * b.1;
	}
}
