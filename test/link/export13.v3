//@defined A .text.virgil.base, A .data.virgil.base, T foo; .entry
export def foo(b: byte, s: i16, i: i32, l: i64, f: float, d: double, p: Pointer, space: Pointer, which: int) {
	match (which) {
		0 => space.store<byte>(byte.view(b + 1));
		1 => space.store<i16 >(i16 .view(s + 1));
		2 => space.store<i32 >(i32 .view(i + 1));
		3 => space.store<i64 >(i64 .view(l + 1));
		4 => space.store<float  >(f + 1);
		5 => space.store<double >(d + 1);
		6 => space.store<Pointer>(p + 1);
		_ => ;
	}
}
