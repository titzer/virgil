//@run 0=75; 1=89; 2=89; 3=75
//@defined A .text.virgil.base, A .data.virgil.base, T vmain, U xmeth
// 89 == 'Y'  75 == 'N'
component import02 {
	def g(x: int) -> int { return x - 1; }
	def f1 = g;
	def f2 = comp.meth;
	def fs = [f1, f2];
}

export def vmain(x: int) -> int {
	var idx = x & 1;
	var y = import02.fs[idx](x);
	return if(y == 1, 89, 75);
}
import "" component comp {
	"xmeth" def meth(x: int) -> int;
}
