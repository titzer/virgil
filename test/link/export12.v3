def foo<T>(x: T) -> T {
	match (x) {
		b: byte => return T.!(byte.view(b + 1));
		s: i16 => return T.!(i16.view(s + 1));
		i: i32 => return T.!(i32.view(i + 1));
		l: i64 => return T.!(i64.view(l + 1));
		f: float => return T.!(f + 1);
		d: double => return T.!(d + 1);
		p: Pointer => return T.!(p + 1);
		_ => return x;
	}
}
