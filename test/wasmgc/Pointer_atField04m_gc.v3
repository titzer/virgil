//@execute = true
layout A {
	+0	f:	int;
	+4	g:	int;
	=8;
}
var rng: Range<byte>;
var r: Ref<A>;

def f1() -> Pointer {
	return Pointer.atRef(r);
}
def f2() -> Pointer {
	return Pointer.atField(r.f);
}
def main() -> bool {
	rng = CiRuntime.forgeRange<byte>(Pointer.NULL + 1024, A.size);
	r = Ref<A>.of(rng);
	return f1() == f2();
}
