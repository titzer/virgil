//@execute 0=!BoundsCheckException; 1=!BoundsCheckException; 2=!BoundsCheckException; 3=!BoundsCheckException; 4=false

layout L {
	+0	x:	u32;
	=4;
}

var x = Array<byte>.new(8);

def main(a: int) -> bool {
	var r1 = Ref<L>.at(x, 0);
	var r2 = Ref<L>.at(x, 4);
	match (a) {
		0 => return Pointer.atElement(x, 0) == Pointer.atRef(r1);
		1 => return Pointer.atElement(x, 0) == Pointer.atRef(r2);
		2 => return Pointer.atElement(x, 4) == Pointer.atRef(r1);
		3 => return Pointer.atElement(x, 4) == Pointer.atRef(r2);
	}
	return false;
}
