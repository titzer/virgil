//@execute 0=false; 1=false; 2=!BoundsCheckException
var a = ["arf", "bla"];
def main(i: int) -> bool {
	def ar = Array<Range<byte>>.new(2);
	for (i < a.length) {
		ar[i] = CiRuntime.forgeRange<byte>(Pointer.NULL + 1024 + i * 128,
						   a[i].length);
	}
	var x = Pointer.atElement(ar[0], 0);
	var y = Pointer.atElement(ar[i], 1);
	return x == y;
}
