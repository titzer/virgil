//@execute 0=false; 1=!BoundsCheckException; 2=!BoundsCheckException; 3=!BoundsCheckException; 4=false
var a: Range<byte> = "arf";
var b: Range<bool> = [true, false];
var c: Range<int> = [0, 1];
var d: Range<void> = null;
def main(i: int) -> bool {
	def ra: Range<byte> = CiRuntime.forgeRange<byte>(Pointer.NULL + 1024, a.length);
	for (i < a.length) ra[i] = a[i];
	def rb: Range<bool> = CiRuntime.forgeRange<bool>(Pointer.NULL + 1024 + 128, b.length);
	for (i < b.length) rb[i] = b[i];
	def rc: Range<int> = CiRuntime.forgeRange<int>(Pointer.NULL + 1024 + 128 + 128, c.length);
	for (i < c.length) rc[i] = c[i];
	def rd: Range<void> = null;
	var x = Pointer.atElement(ra, 0), y: Pointer;
	if (i == 0) y = Pointer.atElement(ra, 2);
	if (i == 1) y = Pointer.atElement(rb, 2);
	if (i == 2) y = Pointer.atElement(rc, 2);
	if (i == 3) y = Pointer.atElement(rd, 2);
	return x == y;
}
