//@execute 0=!BoundsCheckException; 1=!BoundsCheckException; 2=!BoundsCheckException; 3=!BoundsCheckException
class A { }
class B { }
def kA1 = A.new(), kB1 = B.new(), kA2 = A.new();
def main(a: int) -> bool {
	var x = Pointer.atObject(kA1), y: Pointer;
	if (a == 0) y = Pointer.atObject(kA1);
	if (a == 1) y = Pointer.atObject(kB1);
	if (a == 2) y = Pointer.atObject(kA2);
	return x == y;
}
