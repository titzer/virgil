var a: Array<byte> = "arf";
def main(i: int) -> bool {
	def r: Range<byte> = CiRuntime.forgeRange<byte>(Pointer.NULL + 1024, a.length);
	for (i < a.length) r[i] = a[i];
	var x = Pointer.atElement(r, 0), y: Pointer;
	if (i == 0) y = Pointer.atElement(r, 0);
	return x == y;
}

