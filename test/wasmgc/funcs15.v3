//@execute 0=12; 1=13; 2=12
// 0=12; 1=13; 2=12

// Test function type subsumption of a function constant at run time

class A {
	def m() -> int { return 11; }
}

class B extends A {
	def m() -> int { return 12; }
}

def fa1(x: A) -> int { return x.m(); }
def fa2(x: A) -> int { return x.m() + 1; }

def fb(x: B) -> int { return x.m(); }

def xyz(x: B) -> int { return fa1(x); }

def main(i: int) -> int {
	var arr = Array<B -> int>.new(1);
	if (i == 0) { arr[0] = fa1; }
	else if (i == 1) { arr[0] = fa2; }
	else { arr[0] = fb; }
	return arr[0](B.new());
}
