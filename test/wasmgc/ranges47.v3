//@execute (10,3,1)=4; (10,5,4)=7

def main(len: int, count: int, offset: int) -> int {
	var r: Range<i32>;
	if (len == 0) {
		var p = Pointer.NULL;
		for (i < len) { (p + i * 4).store<i32>(i32.!(i)); }
		r = CiRuntime.forgeRange<i32>(p + 3 * 4, count);
	} else {
		var a = Array<i32>.new(len);
		for (i < len) a[i] = i32.!(i);
		r = a[3 ..+ count];
	}
	return int.!(r[offset]);
}
