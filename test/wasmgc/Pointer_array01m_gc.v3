//@execute = 160
def a: Array<byte> = ['\x10', '\x20', '\x30', '\x40'];
def main() -> int {
	def r: Range<byte> = CiRuntime.forgeRange<byte>(Pointer.NULL + 1024, a.length);
	for (i < a.length) r[i] = a[i];
	var p = Pointer.atContents(r), sum = 0;
	for (i = 0; i < 4; (i++, p = Pointer.+(p, 1))) {
		sum = sum + Pointer.load<byte>(p);
	}
	return sum;
}
