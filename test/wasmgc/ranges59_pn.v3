//@execute (10,3,1)=4; (10,5,4)=7; (-10,3,1)=4; (-10,5,4)=7

def main(len: int, count: int, offset: int) -> int {
	var r: Range<double>;
	if (len < 0) {
		var p = Pointer.NULL;
		for (i < -len) { (p + i * 8).store<double>(double.!(i)); }
		r = CiRuntime.forgeRange<double>(p + 3 * 8, count);
	} else {
		var a = Array<double>.new(len);
		for (i < len) a[i] = double.!(i);
		r = a[3 ..+ count];
	}
	return int.!(r[offset]);
}
