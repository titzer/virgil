//@execute 0=!BoundsCheckException; 1=!BoundsCheckException; 2=!BoundsCheckException; 3=!BoundsCheckException; 4=!BoundsCheckException
class A {
	var f: int;
	var g: int;
}
var L = [A.new(), A.new()];
def main(a: int) -> bool {
	var x = Pointer.atField(L[0].f), y = Pointer.NULL;
	if (a == 0) y = Pointer.atField(L[1].f);
	if (a == 1) y = Pointer.atField(L[1].g);
	if (a == 2) y = Pointer.atField(L[0].f);
	if (a == 3) y = Pointer.atField(L[0].g);
	return x == y;
}
