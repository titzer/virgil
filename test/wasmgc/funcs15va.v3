// Test various function subsumptions to see how they differ

class A {
	def m() -> int { return 11; }
}

class B extends A {
	def m() -> int { return 12; }
}

def fai(x: A, j: int) -> int { return x.m() + j; }
def fbi(x: B, j: int) -> int { return fai(x, j); }
def fb(x: B) -> int { return x.m() + 5; }

def main(i: int) -> int {
	var arr = Array<B -> int>.new(1);
	if (i == 1) { arr[0] = fbi(_, 3); }
	else { arr[0] = fb; }
	return arr[0](B.new());
}
