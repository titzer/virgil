// Test function type subsumption of a function constant in a component

class A {
	def m() -> int { return 11; }
}

class B extends A {
	def m() -> int { return 12; }
}

def fa(x: A) -> int { return x.m(); }

def fb(x: B) -> int { return x.m(); }

def cast<T,U>(x: T) -> U {
	return U.!(x);
}

def main(i: int) -> int {
//	var myfb: B -> int = fa;
//	var myfa = cast<A -> int, B -> int>(myfb);
	var myfa = cast<A -> int, B -> int>(fa);
	return myfa(B.new());
}
