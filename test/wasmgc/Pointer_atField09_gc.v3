//@execute 0=!BoundsCheckException; 22=!BoundsCheckException; 90=!BoundsCheckException; 128=!BoundsCheckException

layout L {
	+0	x:	byte;
	+1	y:	byte;
	+11	z:	u64;
	=32;
}

def storage = Array<byte>.new(4 * L.size);

def main(a: int) -> int {
	var r = Ref<L>.at(storage, a);
	var p = Pointer.atField(r.z);
	return int.!(p - Pointer.atContents(storage));
}