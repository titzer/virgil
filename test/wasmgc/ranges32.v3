//@execute (10,3,1)=4; (10,5,4)=7

def main(len: int, count: int, offset: int) -> int {
	var r: Range<u8>;
	if (len == 0) {
		var p = Pointer.NULL;
		for (i < len) { (p + i * 1).store<u8>(u8.!(i)); }
		r = CiRuntime.forgeRange<u8>(p + 3 * 1, count);
	} else {
		var a = Array<u8>.new(len);
		for (i < len) a[i] = u8.!(i);
		r = a[3 ..+ count];
	}
	return int.!(r[offset]);
}
