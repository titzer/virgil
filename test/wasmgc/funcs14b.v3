//@execute 0=12; 1=19

// Test function type subsumption of a function constant in a component,
// implemented with an adapter

class A {
	def m() -> int { return 11; }
}

class B extends A {
	def m() -> int { return 12; }
}

def fa(x: A) -> int { return x.m(); }

def fb(x: B) -> int { return x.m() + 7; }

def adapter(f: A -> int, b: B) -> int { return f(b); }

def arr: Array<B -> int> = [adapter(fa, _), fb];

def main(i: int) -> int {
	return arr[i](B.new());
}
