//@execute (10,2,3,1,4)=4; (10,3,5,4,6)=6; (-10,2,3,1,4)=4; (-10,3,5,4,6)=6

def main(len: int, t: int, count: int, offset: int, value: int) -> int {
	var r: Range<u32>;
	if (len < 0) {
		var p = Pointer.NULL;
		for (i < -len) { (p + i * 4).store<u32>(u32.!(i)); }
		r = CiRuntime.forgeRange<u32>(p + t * 4, count);
	} else {
		var a = Array<u32>.new(len);
		for (i < len) a[i] = u32.!(i);
		r = a[t ..+ count];
	}
	return int.!(r[offset] = u32.!(value));
}
