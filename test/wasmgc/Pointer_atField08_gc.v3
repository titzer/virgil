//@execute = !BoundsCheckException
layout A {
	+0	f:	int;
	+4	g:	int;
	=8;
}
var array = Array<byte>.new(A.size);
var r = Ref<A>.of(array);

def f1(r: Ref<A>) -> Pointer {
	return Pointer.atRef(r);
}
def f2(r: Ref<A>) -> Pointer {
	return Pointer.atField(r.f);
}
def main() -> bool {
	return f1(r) == f2(r);
}
