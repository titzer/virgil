	return y;
}

def ff32(n: int, i: int, x: Array<float>) -> Array<float> {
	var y: Array<float> = [x[i + n]];
	return y;
}

def ff64(n: int, i: int, x: Array<double>) -> Array<double> {
	var y: Array<double> = [x[i + n]];
	return y;
}

def main(args: Array<string>) -> int {
	var n = args.length;
	var xi7 = fi7(n, n - 1, [3i7]);
	var xi8 = fi8(n, n - 1, [3i8]);
	var xi15 = fi15(n, n - 1, [3i15]);
	var xi16 = fi16(n, n - 1, [3i16]);
	var xi31 = fi31(n, n - 1, [3i31]);
	var xi32 = fi32(n, n - 1, [3i32]);
	var xi63 = fi63(n, n - 1, [3i63]);
	var xi64 = fi64(n, n - 1, [3i64]);
	var isum = xi7[0] + xi8[0] + xi15[0] + xi16[0] + xi31[0] + xi32[0] + xi63[0] + xi64[0];
	var xu7 = fu7(n, n - 1, [3u7]);
	var xu8 = fu8(n, n - 1, [3u8]);
	var xu15 = fu15(n, n - 1, [3u15]);
	var xu16 = fu16(n, n - 1, [3u16]);
	var xu31 = fu31(n, n - 1, [3u31]);
	var xu32 = fu32(n, n - 1, [3u32]);
	var xu63 = fu63(n, n - 1, [3u63]);
	var xu64 = fu64(n, n - 1, [3u64]);
	var usum = xu7[0] + xu8[0] + xu15[0] + xu16[0] + xu31[0] + xu32[0] + xu63[0] + xu64[0];
	var xf32 = ff32(n, n - 1, [3.0f]);
	var xf64 = ff64(n, n - 1, [3.0d]);
	var fsum = xf32[0] + xf64[0];
	return int.view(isum + i64.view(usum) + i64.!(fsum));
}
