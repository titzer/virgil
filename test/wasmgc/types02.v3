// A test of value types as array element types

def fi7(n: int, i: int, x: Array<i7>) -> Array<i7> {
	var y: Array<i7> = [x[i + n]];
	return y;
}

def fi8(n: int, i: int, x: Array<i8>) -> Array<i8> {
	var y: Array<i8> = [x[i + n]];
	return y;
}

