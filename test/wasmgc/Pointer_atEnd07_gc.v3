//@execute 0=!BoundsCheckException; 1=!BoundsCheckException
class A(x: int, y: int) { }

def ka = A.new(33, 44);
def kb = A.new(66, 77);

def main(a: int) -> int {
	var o = if(a > 0, kb, ka);

	var p0 = Pointer.atObject(o);
	var p1 = Pointer.atField(o.x);
	var p2 = Pointer.atField(o.y);
	var p3 = Pointer.atEnd(o);

	if (p0 >= p1) return -1;
	if (p0 >= p2) return -2;
	if (p0 >= p3) return -3;
	if (p1 >= p3) return -4;
	if (p2 >= p3) return -5;
	return 67;
}
