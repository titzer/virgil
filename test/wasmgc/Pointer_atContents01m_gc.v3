//@execute 0=true; 1=false; 2=false; 3=false; 4=false
var a: Array<byte> = "arf";
var b: Array<bool> = [true];
var c: Array<int> = [0];
var d: Array<void> = null;
def main(i: int) -> bool {
	def ra: Range<byte> = CiRuntime.forgeRange<byte>(Pointer.NULL + 1024, a.length);
	for (i < a.length) ra[i] = a[i];
	def rb: Range<bool> = CiRuntime.forgeRange<bool>(Pointer.NULL + 1024 + 128, b.length);
	for (i < b.length) rb[i] = b[i];
	def rc: Range<int> = CiRuntime.forgeRange<int>(Pointer.NULL + 1024 + 128 + 128, c.length);
	for (i < b.length) rb[i] = b[i];
	def rd: Range<void> = null;
	var x = Pointer.atContents(ra), y: Pointer;
	if (i == 0) y = Pointer.atContents(ra);
	if (i == 1) y = Pointer.atContents(rb);
	if (i == 2) y = Pointer.atContents(rc);
	if (i == 3) y = Pointer.atContents(rd);
	return x == y;
}

