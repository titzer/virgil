//@execute 1=17; 2=15

class A {
	def m() -> int { return 11; }
}

class B extends A {
	def m() -> int { return 12; }
}

def fa(x: A) -> int { return x.m() + 3; }
def fb(x: B) -> int { return x.m() + 5; }

class Cls {
	def fa(x: A) -> int { return x.m() + 3; }
}

def main(i: int) -> int {
	var arr = Array<B -> int>.new(1);
	var cls = Cls.new();
	if (i == 2) { arr[0] = cls.fa; }
	else { arr[0] = fb; }
	return arr[0](B.new());
}
