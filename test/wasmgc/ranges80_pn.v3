def f(i: int) -> u64 {
	return u64.!(i);
}

def main(len: int, t: int, count: int, offset: int, value: int) -> int {
	var r: Range<u64>;
	if (len < 0) {
		var p = Pointer.NULL;
		for (i < -len) { (p + i * 8).store<u64>(u64.!(i)); }
		r = CiRuntime.forgeRange<u64>(p + t * 8, count);
	} else {
		var a = Array<u64>.new(len);
		for (i < len) a[i] = u64.!(i);
		r = a[t ..+ count];
	}
	return int.!(r[offset] = u64.!(value));
}
