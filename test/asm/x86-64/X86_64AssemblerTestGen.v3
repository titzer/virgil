		var name = buf.puts("j").puts(cond.name).puts(" short").extract();
		do_b(name, asm.jc_rel(cond, _));
		name = buf.puts("j").puts(cond.name).extract();
		do_nb(name, asm.jc_rel(cond, _));
	}
	do_b("jmp short", asm.jmp_rel);
	do_nb("jmp", asm.jmp_rel);
}

