def main(a: bool) -> int {
	return if(a, call0(m0, m1), call0(m1, m0));	
}
def m0(x: int, y: int, z: int, w: int) -> int {
	return (x << 6) | (y << 4) | (z << 2) | w;
}
def m1(x: int, y: int, z: int, w: int) -> int {
	return (x << 2) | (y << 4) | (z << 6) | w;
}
def call0(f0: (int, int, int, int) -> int, f1: (int, int, int, int) -> int) -> int {
	return f0(0, 1, 2, 3);
}
