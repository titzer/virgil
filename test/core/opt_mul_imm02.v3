//@execute 0=0; 1=255; 2=510; 10=2550; 100=25500
def main(a: int) -> int {
	return f1(a) + f2(a) + f3(a) + f4(a) + f5(a);
}
def f1(a: int) -> int { return a * 16; }
def f2(a: int) -> int { return a * 32; }
def f3(a: int) -> int { return a * 64; }
def f4(a: int) -> int { return a * 128; }
def f5(a: int) -> int { return a * 15; }
