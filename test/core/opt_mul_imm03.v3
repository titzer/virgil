def main(a: int) -> int {
	return f1(a) + f2(a) + f3(a) + f4(a) + f5(a);
}
def f1(a: int) -> int { return a * 256; }
def f2(a: int) -> int { return a * 127; }
def f3(a: int) -> int { return a * 512; }
def f4(a: int) -> int { return a * 64; }
def f5(a: int) -> int { return a * 64; }
