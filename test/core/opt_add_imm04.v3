//@execute 0=1500; 1=1505; -1=1495; -10=1450; 100=2000
def main(a: int) -> int {
    return f1(a) + f2(a) + f3(a) + f4(a) + f5(a);
}
def f1(a: int) -> int { return a + 17; }     // non-power-of-two
def f2(a: int) -> int { return a + 33; }     // non-power-of-two
def f3(a: int) -> int { return a + 128; }    // power-of-two
def f4(a: int) -> int { return a + 512; }    // power-of-two
def f5(a: int) -> int { return a + 810; }    // non-power-of-two
