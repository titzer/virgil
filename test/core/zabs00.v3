//@execute (0, 3)=-2; (1, 3)=8; (2, 3)=-2; (3, 4)=9; (4, 1)=2
def f(x: int) -> int { return x + 5; }
def g(x: int) -> int { return x - 5; }

def main(which: int, x: int) -> int {
	var callf: bool;
	var callg: bool;
	match (which) {
		0 => { callf = false; callg = true;  }
		1 => { callf = true;  callg = false; }
		2 => { callf = false; callg = true;  }
		3 => { callf = true;  callg = false; }
		_ => { callf = true;  callg = true;  }
	}
	return if(callf, f(x)) + if(callg, g(x));
}
