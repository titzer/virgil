//@execute 0=-1; 100=-7; 4444444=-6
def main(a: int) -> int {
	var x1 = a + 1;
	var x2 = a + 2;
	var x3 = a + 3;
	var x4 = a + 4;
	var x5 = a + 5;
	var x6 = a + 6;
	var x7 = a + 7;
	var x8 = a + 8;
	while (a-- > 0) {
		var y = x1 + x2 + x3 + x4 + x5 + x6 + x7 + x8 + a;
		if (y > 100) {
			var z1 = a + 1;
			var z2 = a + 2;
			var z3 = a + 3;
			var z4 = a + 4;
			var z5 = a + 5;
			var z6 = a + 6;
			var z7 = a + 7;
			var z8 = a + 8;
			var zx = z1 + z2 + z3 + z4 + z5 + z6 + z7 + z8;
			var zy = z1 + z2 + z3 + z4 + z5 + z6 + z7 + z8;
			a -= (zx & zy & z7 & 0xFFFF);
		} else {
			var z1 = a + 1;
			var z2 = a + 2;
			var z3 = a + 3;
			var z4 = a + 4;
			var z5 = a + 5;
			var z6 = a + 6;
			var z7 = a + 7;
			var z8 = a + 8;
			var zx = z1 + z2 + z3 + z4 + z5 + z6 + z7 + z8;
			var zy = z1 + z2 + z3 + z4 + z5 + z6 + z7 + z8;
			a -= (zx & zy & z7 & 0xFF_FFFF);
		}
	}
	return a;
}
