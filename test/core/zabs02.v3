//@execute (0, 1, 2)=1; (1, 1, 2)=1; (2, 1, 2)=1; (3, 1, 2)=4; (0, 44, 55)=44; (1, 55, 66)=55; (2, 66, 77)=66; (3, 77, 88)=176
def main(which: int, x: int, y: int) -> int {
	var f: int, g: int;
	var choosef: bool;
	var chooseg: bool;
	match (which) {
		0 => { f = x; g = x; choosef = false; chooseg = true;  }
		1 => { f = x; g = y; choosef = true;  chooseg = false; }
		2 => { f = y; g = x; choosef = false; chooseg = true;  }
		_ => { f = y; g = y; choosef = true;  chooseg = true;  }
	}
	return if(choosef, f) + if(chooseg, g);
}
