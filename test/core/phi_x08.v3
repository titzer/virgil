//@execute -10=-52; -1=20; 0=28; 2=44; 9=100
def main(a: int) -> int {
	var x0 = a, x1 = a + 1, x2 = a + 2, x3 = a + 3, x4 = a + 4, x5 = a + 5, x6 = a + 6, x7 = a + 7;
	var t = if(a < 0, (x0, x1, x2, x3, x4, x5, x6, x7), (x7, x6, x5, x4, x3, x2, x1, x0));
	return t.0 + t.1 + t.2 + t.3 + t.4 + t.5 + t.6 + t.7;
}
