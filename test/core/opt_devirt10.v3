//@execute =!NullCheckException
class A {
	def m() -> int { return 11; }
}
class B extends A {
	def m() -> int { return 21; }
}
var x: A = A.new();

def foo(arg: int) -> int {
	var z: A;
	if (arg == 1) z = x;
	return get(A.m, z);
}
def get(f: A -> int, o: A) -> int {
	return f(o);
}

def main() {
	foo(0);
}
