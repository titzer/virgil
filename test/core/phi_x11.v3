def main(a: int) -> int {
	var x0 = a, x1 = a + 1, x2 = a + 2, x3 = a + 3, x4 = a + 4, x5 = a + 5, x6 = a + 6, x7 = a + 7, x8 = a + 8, x9 = a + 9, x10 = a + 10;
	var t = if(a < 0, (x0, x1, x2, x3, x4, x5, x6, x7, x8, x9, x10), (x10, x9, x8, x7, x6, x5, x4, x3, x2, x1, x0));
