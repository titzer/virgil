//@execute 0=131930; 1=131935; -1=131925; -10000000=-49868070; 1122334455=1316836909
def main(a: int) -> int {
    return f1(a) + f2(a) + f3(a) + f4(a) + f5(a);
}
def f1(a: int) -> int { return a + 4096; }     // 2^12
def f2(a: int) -> int { return a + 8191; }     // max 13-bit value
def f3(a: int) -> int { return a + 10000; }    // non-power
def f4(a: int) -> int { return a + 32767; }    // 2^15-1
def f5(a: int) -> int { return a + 76876; }    // large non-power
