//@execute 0=1083; 1=1344; 4=2287
def sp(v: int) -> int #no-inline {
	return v + 11;
}

def main(a: int) -> int {
	var base = a * 7 + 9;
	var x1 = 0;

	if ((a & 1) == 0) {
		var k = sp(base);
		x1 = k * 2 + 1;
	} else {
		x1 = base * 3 + 5;
	}

	var z = (x1 + 17) * 13 + (base + 3) * 5;
	return z + x1 * 7 - base * 2;
}
