//@execute 0=!NullCheckException; 1=!NullCheckException; -1=!NullCheckException
class A {
	def m() -> int { return 14; }
}
class B extends A {
	def m() -> int { return 12; }
}

def main(a: int) -> int {
	var f: void -> int;
	if (a < 0) {
		var o: A = null;
		f = o.m;
	} else {
		var o: B = null;
		f = o.m;
	}
	return f();
}
