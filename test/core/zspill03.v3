def sp(v: int) -> int #no-inline {
	return v ^ 12345;
}
def main(a: int) -> int {
	var x0 = a * 100 + 7;

	// Payload temps kept live across the branch and merge.
	var p0 = x0 + 1;
	var p1 = p0 * 3 + 5;
	var p2 = p1 * 7 + 11;
	var p3 = p2 * 13 + 17;

	var x1 = 0;
	if ((a & 1) == 0) {
		var t = sp(x0);
		x1 = t + p1;
