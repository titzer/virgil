def main(a: int) -> int {
	return f1(a) + f2(a) + f3(a) + f4(a) + f5(a);
}
def f1(a: int) -> int { return a / 100; }
def f2(a: int) -> int { return a / 200; }
def f3(a: int) -> int { return a / 300; }
def f4(a: int) -> int { return a / 400; }
def f5(a: int) -> int { return a / 500; }
