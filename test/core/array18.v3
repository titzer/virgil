//@execute 0=42; 1=10; 2=11; 3=12; 4=13; 5=14; 6=15; 7=42

component array18 {
    var matrix: Array<Array<int>> = compute();

    def compute() -> Array<Array<int>> {
	var matrix = Array<Array<int>>.new(2);
	var i = 0, cnt = 0;
	while (i < 2) {
	    matrix[i] = Array<int>.new(3);
	    var j = 0;
	    while (j < 3) {
            	matrix[i][j] = 10 + cnt;
		j = j + 1;
		cnt = cnt + 1;
	    }
	    i = i + 1;
        }
	return matrix;
    }

    def main(arg: int) -> int {
	if (arg == 1) return matrix[0][0];
	if (arg == 2) return matrix[0][1];
	if (arg == 3) return matrix[0][2];
	if (arg == 4) return matrix[1][0];
	if (arg == 5) return matrix[1][1];
	if (arg == 6) return matrix[1][2];
	return 42;
    }
}
