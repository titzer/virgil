//@execute (0, 1, 2, 3)=-7; (1, 1, 2, 3)=13; (2, 1, 2, 3)=-7; (3, 1, 2, 4)=8; (4, 12, 2, 1)=2

def f(i: int) -> int { return i + 10; }
def g(i: int) -> int { return i - 10; }

def main(which: int, val1: int, val2: int, x: int) -> int {
	var callf: bool;
	var callg: bool;
	match (which) {
		0 => { callf = false; callg = true;  }
		1 => { callf = true;  callg = false; }
		2 => { callf = false; callg = true;  }
		3 => { callf = true;  callg = true; }
		_ => { callf = true;  callg = true;  }
	}
	return if(callf, f(x)) + if(callg, g(x));
}
