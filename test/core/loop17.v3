//@execute 0=-1; 100=-308; 4444444=-31795
def main(a: int) -> int {
	var x1 = a + 1;
	var x2 = a + 2;
	var x3 = a + 3;
	var x4 = a + 4;
	while (a-- > 0) {
		var y = x1 + x2 + x3 + x4 + a;
		if (y > 100) {
			var z1 = a + 1;
			var z2 = a + 2;
			var z3 = a + 3;
			var z4 = a + 4;
			var z5 = z1 + z2 + z3 + z4;
			a -= (z5 & 0xFFFF);
		} else {
			var z1 = a + 1;
			var z2 = a + 2;
			var z3 = a + 3;
			var z4 = a + 4;
			var z5 = z1 + z2 + z3 + z4;
			a -= (z5 & 0xFFF_FFFF);
		}
	}
	return a;
}