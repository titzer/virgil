def sum(a: int, b: int, c: int, d: int, e: int, f: int) -> int {
	return a + b + c + d + e + f;
}
def pos0(a: int) -> int {
	return sum(  a << 20, 0x01_0000, 0x00_1000, 0x00_0100, 0x00_0010, 0x00_0001);
}
def pos1(a: int) -> int {
	return sum(0x10_0000,   a << 16, 0x00_1000, 0x00_0100, 0x00_0010, 0x00_0001);
}
def pos2(a: int) -> int {
	return sum(0x10_0000, 0x01_0000,   a << 12, 0x00_0100, 0x00_0010, 0x00_0001);
}
def pos3(a: int) -> int {
	return sum(0x10_0000, 0x01_0000, 0x00_1000,    a << 8, 0x00_0010, 0x00_0001);
}
def pos4(a: int) -> int {
	return sum(0x10_0000, 0x01_0000, 0x00_1000, 0x00_0100,    a << 4, 0x00_0001);
}
def pos5(a: int) -> int {
	return sum(0x10_0000, 0x01_0000, 0x00_1000, 0x00_0100, 0x00_0010,         a);
}
def main(a: int) -> int {
	return pos0(a) + pos1(a) + pos2(a) + pos3(a) + pos4(a) + pos5(a);
}
