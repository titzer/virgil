//@execute 0=4995; 1=5000; -1=4990; -10=4945; 100=5495
def main(a: int) -> int {
    return f1(a) + f2(a) + f3(a) + f4(a) + f5(a);
}
def f1(a: int) -> int { return a + 123; }     // non-power-of-two
def f2(a: int) -> int { return a + 777; }     // non-power-of-two
def f3(a: int) -> int { return a + 1023; }    // edge of immediate range
def f4(a: int) -> int { return a + 1024; }    // power-of-two
def f5(a: int) -> int { return a + 2048; }    // power-of-two
