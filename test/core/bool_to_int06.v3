//@execute true=true; false=true
def main(b:bool) -> bool {
    def expectedByte: byte = if(b, byte.!(1), byte.!(0));
    def arr = Array<byte>.new(4);
    arr[0] = byte.!(0);
    arr[1] = byte.view(b);
    arr[2] = byte.!(0);
    arr[3] = byte.view(b);
    return arr[1] == expectedByte && arr[3] == expectedByte;
}