//@execute 0=-65535; 10=-65485; 100=-65035; 1000=-60535; -1000=-70535
def main(a: int) -> int {
	return f1(a) + f2(a) + f3(a) + f4(a) + f5(a);
}
def f1(a: int) -> int { return a - 4096; }
def f2(a: int) -> int { return a - 8192; }
def f3(a: int) -> int { return a - 16384; }
def f4(a: int) -> int { return a - 32768; }
def f5(a: int) -> int { return a - 4095; }
