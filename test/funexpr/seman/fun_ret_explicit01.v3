//@execute 0=6; 1=1; 2=42
def add = fun (x: int, y: int) -> int => x + y;
def isPositive = fun (x: int) -> bool => x > 0;
def getFirst = fun (arr: Array<int>) -> int => arr[0];

def main(a: int) -> int {
    if (a == 0) return add(2, 4);
    if (a == 1) return if(isPositive(5), 1, 0);
    return getFirst([42, 99]);
}