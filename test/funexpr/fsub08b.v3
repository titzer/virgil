//@execute (0, 0, 0)=25; (0, 1, 0)=25; (0, 2, 0)=13; (1, 0, 0)=25; (1, 1, 0)=25; (1, 2, 0)=13; (2, 0, 0)=12; (2, 1, 0)=12; (2, 2, 0)=0; (3, 0, 0)=35; (3, 1, 0)=35; (3, 2, 0)=23
class A { def m() => 11; }
class B extends A { def m() => 12; }
class C extends B { def m() => 13; }

def fa1(x: A, j: int) => x.m() + j;
def fa2(x: A, j: int) => x.m() - j;
def fb(x: B, j: int) => x.m() * j;
def fc(x: C, j: int) => x.m() + j + 10;

def a2b1(x: B, i: int) => fa1(x, i);
def a2b2(x: B, i: int) => fa2(x, i);
def a2c1(x: C, i: int) => fa1(x, i);
def a2c2(x: C, i: int) => fa2(x, i);
def b2c(x: C, i: int) => fb(x, i);

def main(i1: int, i2: int, j: int) -> int {
	return [a2c1, a2c2, b2c, fc][i1](C.new(), j) + [a2b1, a2b2, fb][i2](B.new(), j);
}
