//@execute 0=12; 1=13
class A { def m() => 11; }
class B extends A { def m() => 12; }

def fa1(x: A) => x.m();
def fa2(x: A) => x.m() + 1;
def fb(x: B) => x.m();

def main(i: int) -> int {
	var arr = Array<B -> int>.new(1);
	if (i == 0) { arr[0] = fa1; }
	else if (i == 1) { arr[0] = fa2; }
	else { arr[0] = fb; }
	return arr[0](B.new());
}
