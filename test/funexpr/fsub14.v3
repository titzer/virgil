//@execute 0=13; 1=14
class C { def n() => 13; }
class D extends C { def n() => 13; }

class Cls {
	def fd1() -> (D -> int) { return adapter(g1, _); }
	def fd2() -> (D -> int) { return adapter(g2, _); }
	def g1(x: C) => x.n();
	def g2(x: C) => x.n() + 1;
}

def adapter(f: C -> int, d: D) => f(d);

def main(i: int) -> int {
	var cls = Cls.new();
	var arrf: Array<() -> (D -> int)> = [cls.fd1, cls.fd2];
	return arrf[i]()(D.new());
}
