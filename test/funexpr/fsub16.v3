//@execute 0=13; 1=14
class C { def n() => 13; }
class D extends C { def n() => 14; }
class Cls {
	def fc() => C.new();
	def fd() => D.new();
}

def adapter(f: () -> D, v: ()) -> C {
	return f();
}

def main(i: int) -> int {
	var cls = Cls.new();
	var arrf: Array<() -> C> = [cls.fc, adapter(cls.fd, _)];
	return arrf[i]().n();
}
