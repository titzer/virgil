//@execute (0, 10)=22; (1, 10)=22; (2, 2)=24
class A { def m() => 11; }
class B extends A { def m() => 12; }

def fa1(x: A, j: int) => x.m() + j;
def fa2(x: A, j: int) => x.m() - j;
def fb(x: B, j: int) => x.m() * j;

def adapter1(b: B, j: int) => fa1(b, j);
def adapter2(b: B, j: int) => fa1(b, j);

def main(i: int, j: int) -> int {
	var arr = Array<(B, int) -> int>.new(1);
	if (i == 0) { arr[0] = adapter1; }
	else if (i == 1) { arr[0] = adapter2; }
	else { arr[0] = fb; }
	return arr[0](B.new(), j);
}
