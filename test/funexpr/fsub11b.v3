//@execute (0, 10)=35; (1, 10)=-11; (2, 2)=26; (3, 3)=29
class A { def m() => 11; }
class B extends A { def m() => 12; }
class C { def n() => 13; }
class D extends C { def n() => 13; }

def fac(x: A, y: C, j: int) => x.m() + y.n() + j;
def fbc(x: B, y: C, j: int) => x.m() - y.n() - j;
def fad(x: A, y: D, j: int) => x.m() + y.n() + j - 1;
def fbd(x: B, y: D, j: int) => x.m() + y.n() + j + 1;

def main(i: int, j: int) -> int {
	var x = [fac, fbc, fad, fbd];
	return x[i](B.new(), D.new(), j);
}
