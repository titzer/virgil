type Option<T> {
        case None;
        case Some(val: T) #unboxed;
}

def f(x: Option<int>) -> int {
        match (x) {
                None => return 0;
                Some(val) => return val;
        }
}

def main() -> int {
	var n = Option<int>.None;
        var s = Option<int>.Some(10);
        
        if (f(n) != 0) return -1;
        if (f(s) != 10) return -2;
        return 3;
}