type T #unboxed {
	case A(f: void -> int);
	case B(a: string, b: string);
}

def ts = [T.A(f1), T.A(f2), T.B("1", "2"), T.B("-1", "-33")];

def f1() -> int { return 1; }
def f2() -> int { return 2; }

def main(a: int) -> int {
	var s = 9;
	for (i < 20) {
		var u = [T.A(null), T.B(null, Array<byte>.new(2))];
		s += count(ts[a]);
	}
	return s;
}

def count(t: T) -> int {
	match (t) {
		A(f) => return f();
		B(a, b) => return a.length + b.length;
	}
}