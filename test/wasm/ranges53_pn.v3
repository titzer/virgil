//@execute (10,3,1,4)=4; (10,5,4)=7

def main(len: int, count: int, offset: int) -> int {
	var r: Range<i64>;
	if (len == 0) {
		var p = Pointer.NULL;
		for (i < len) { (p + i * 8).store<i64>(i64.!(i)); }
		r = CiRuntime.forgeRange<i64>(p + 3 * 8, count);
	} else {
		var a = Array<i64>.new(len);
		for (i < len) a[i] = i64.!(i);
		r = a[3 ..+ count];
	}
	return int.!(r[offset]);
}
