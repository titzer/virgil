//@execute 0=5; 1=5
def targets = [LinuxTarget.new(), LinuxTarget.new()];

class LinuxTarget {
	def linuxComponent = Kernel_TypeCon.new(Kernel.LINUX);
}
enum Kernel(typename: string) {
	UNKNOWN("Unknown"), LINUX("Linux"), DARWIN("Darwin")
}
class Kernel_TypeCon(kernel: Kernel) extends TypeCon {
	new() super(kernel.typename) { }
}
class TypeCon(name: string) {
}

def main(a: int) -> int {
	return targets[a].linuxComponent.name.length;
}