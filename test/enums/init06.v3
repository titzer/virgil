//@execute 0=5; 1=5
def targets = [Target.new(), Target.new()];

class Target {
	def typecon = TypeCon.new(Kernel.LINUX);
}
enum Kernel(typename: string) {
	UNKNOWN("Unknown"), LINUX("Linux"), DARWIN("Darwin")
}
class TypeCon(kernel: Kernel) {
	def name = kernel.typename;
}

def main(a: int) -> int {
	return targets[a].typecon.name.length;
}
