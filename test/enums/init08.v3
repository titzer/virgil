//@execute false=6; true=5
def A = TypeCon.new(Kernel.LINUX);
def B = TypeCon.new(Kernel.DARWIN);
enum Kernel(typename: string) {
	UNKNOWN("Unknown"), LINUX("Linux"), DARWIN("Darwin")
}
class TypeCon(kernel: Kernel) {
	def name = kernel.typename;
}

def main(a: bool) -> int {
	var t = if(a, A, B);
	return t.name.length;
}
