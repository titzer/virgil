//@execute 0=5; 1=5
def targets = [LinuxTarget.new(), LinuxTarget.new()];

class LinuxTarget {
	def linuxComponent = Kernel_TypeCon.new(Kernel.LINUX);
}
enum Kernel(typename: string) {
	UNKNOWN("Unknown"), LINUX("Linux"), DARWIN("Darwin")
}
class Kernel_TypeCon(kernel: Kernel) extends Member_TypeCon {
	new() super(kernel.typename) { }
}
class Member_TypeCon extends TypeCon {
	new(name: string) super(name) { }
}
var UID = 0;
class TypeCon {
	def uid  = UID++;		// unique id for type constructor
	def name: string;		// name of the type

	new(name) { }
}

def main(a: int) -> int {
	return targets[a].linuxComponent.name.length;
}