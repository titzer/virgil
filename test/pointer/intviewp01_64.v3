//@execute 0=false; 1=true; 2=true
def A1 = [1, 2, 3];
def A2 = [3, 4, 5];
def p1() -> Pointer {
    var ptr = Pointer.atContents(A1);
    return ptr;
}
def p2() -> Pointer {
    var ptr = Pointer.atContents(A2);
    return ptr;
}
def main(a: int) -> bool {
    match (a) {
        0 => return (long.view<Pointer>(p1()) == long.view<Pointer>(p2()));
        1 => return (long.view<Pointer>(p1()) == long.view<Pointer>(Pointer.atContents(A1)));
        2 => return (long.view<Pointer>(p2()) == long.view<Pointer>(Pointer.atContents(A2)));
        _ => return false;
    }
}