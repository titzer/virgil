//@execute 0=false; 1=true; 2=true
def A1: Range<byte> = "arf";
def A2: Range<byte> = "arf";
def p1() -> Pointer {
    var ptr = Pointer.atContents(A1);
    return ptr;
}
def p2() -> Pointer {
    var ptr = Pointer.atContents(A2);
    return ptr;
}
def main(a: int) -> bool {
    match (a) {
        0 => return (int.view<Pointer>(p1())) == (int.view<Pointer>(p2()));
        1 => return (int.view<Pointer>(p1())) == (int.view<Pointer>(Pointer.atContents(A1)));
        2 => return (int.view<Pointer>(p2())) == (int.view<Pointer>(Pointer.atContents(A2)));
        _ => return false;
    }
}