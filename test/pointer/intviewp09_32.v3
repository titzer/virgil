//@execute 0=true; 1=false; 2=false
def A1: Range<byte> = "vgl";
def main(a: int) -> bool {
    match (a) {
        0 => return (int.view<Pointer>(Pointer.atElement(A1, 0)))  == (int.view<Pointer>(Pointer.atContents(A1)));
        1 => return (int.view<Pointer>(Pointer.atElement(A1, 1)))  == (int.view<Pointer>(Pointer.atContents(A1)));
        2 => return (int.view<Pointer>(Pointer.atElement(A1, 2)))  == (int.view<Pointer>(Pointer.atContents(A1)));
        _ => return false;
    }
}