//@execute 0=true; 1=true; 2=true; 3=true
def A1: Array<int> = [1, 2, 3];
def A2: Array<bool> = [false, true];
def A3: Array<float> = [1F, 3F];
def A4: Array<byte> = "vgl";

def p(a: int) -> long {
    match (a) {
        0 => return long.view<Pointer>(Pointer.atContents(A1));
        1 => return long.view<Pointer>(Pointer.atContents(A2));
        2 => return long.view<Pointer>(Pointer.atContents(A3));
        3 => return long.view<Pointer>(Pointer.atContents(A4));
        _ => return 42;
    }
}

def main(a: int) -> bool {
    match (a) {
        0 =>  return p(a) == (long.view<Pointer>(Pointer.atContents(A1)));
        1 =>  return p(a) == (long.view<Pointer>(Pointer.atContents(A2)));
        2 =>  return p(a) == (long.view<Pointer>(Pointer.atContents(A3)));
        3 =>  return p(a) == (long.view<Pointer>(Pointer.atContents(A4)));
        _ => return false;
    }
}