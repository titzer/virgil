//@execute 0=true; 1=true; 2=true; 3=true
def A1: Array<byte> = "vgl";
def A2: Array<int> = [1, 2];
def A3: Array<bool> = [true, false];
def p1(a: int) -> long {
    return long.view<Pointer>(Pointer.atElement(A1, a));
}
def p2(a: int) -> long {
    return long.view<Pointer>(Pointer.atElement(A2, a));
}
def p3(a: int) -> long {
    return long.view<Pointer>(Pointer.atElement(A3, a));
}
def main(a: int) -> bool {
    match (a) {
        0 => return (long.view<Pointer>(Pointer.atElement(A1, 1)))  == p1(1);
        1 => return (long.view<Pointer>(Pointer.atElement(A1, 2)))  == p1(2);
        2 => return (long.view<Pointer>(Pointer.atElement(A2, 1)))  == p2(1);
        3 => return (long.view<Pointer>(Pointer.atElement(A3, 1)))  == p3(1);
        _ => return false;
    }
}