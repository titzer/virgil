//@execute 0=true; 1=true; 2=true
type A(a: u4, b: u8, c: u19) #unboxed {}

def L = A(1, 2, 3);
def p(a: int) -> long {
    match(a) {
        0 => return long.view<Pointer>(Pointer.atField(L.a));
        1 => return long.view<Pointer>(Pointer.atField(L.b));
        2 => return long.view<Pointer>(Pointer.atField(L.c));
        _ => return 42;
    }
}

def main(a: int) -> bool {
    match (a) {
        0 => return long.view<Pointer>(Pointer.atField(L.a)) == p(0);
        1 => return long.view<Pointer>(Pointer.atField(L.b)) == p(1);
        2 => return long.view<Pointer>(Pointer.atField(L.c)) == p(2);
        _ => return false;
    } 
}