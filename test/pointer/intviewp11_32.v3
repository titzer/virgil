//@execute 0=false; 1=true
def ptr1 = Pointer.NULL;
def ptr2 = Pointer.NULL;
def main(a: int) -> bool {
    def p1 = Pointer.atField(ptr1);
    def p2 = Pointer.atField(ptr2);
    def p3 = Pointer.atField(ptr1);
    match (a) {
        0 => return (int.view<Pointer>(p1) == int.view<Pointer>(p2));
        1 => return (int.view<Pointer>(p1) == int.view<Pointer>(p3));
        _ => return false;
    }

}