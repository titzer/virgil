//@execute 0=0; 1=100; 2=10000; 3=-100000; 4=0
def main(a: int) -> int {
	return int.!(get(a) - Pointer.NULL);
}
def get(a: int) -> Pointer {
	match (a) {
		0 => return Pointer.NULL + 0;
		1 => return Pointer.NULL + 100;
		2 => return Pointer.NULL + 10000;
		3 => return Pointer.NULL + -100000;
		
	}
	return Pointer.NULL;
}
