//@execute 0=true; 1=false; 2=false
def A1: Array<byte> = "vgl";
def p(a: int) -> long {
    var ptr = Pointer.atElement(A1, a);
    return (long.view<Pointer>(ptr));
}
def main(a: int) -> bool {
    match (a) {
        0 => return (long.view<Pointer>(Pointer.atElement(A1, 0)))  == p(0);
        1 => return (long.view<Pointer>(Pointer.atElement(A1, 1)))  == p(2);
        2 => return (long.view<Pointer>(Pointer.atElement(A1, 2)))  == p(1);
        _ => return false;
    }
}