//@execute 0=true; 1=true; 2=true; 3=true
class A {
    def a = 1;
    def b = 2;
}

def L = [A.new(), A.new()];
def p(a: int) -> long {
    match (a) {
        0 => return long.view<Pointer>(Pointer.atField(L[0].a));
        1 => return long.view<Pointer>(Pointer.atField(L[0].b));
        2 => return long.view<Pointer>(Pointer.atField(L[1].a));
        3 => return long.view<Pointer>(Pointer.atField(L[1].b));
        _ => return 42;
    }
}

def main(a: int) -> bool {
    match (a) {
        0 => return long.view<Pointer>(Pointer.atField(L[0].a)) == p(0);
        1 => return long.view<Pointer>(Pointer.atField(L[0].b)) == p(1);
        2 => return long.view<Pointer>(Pointer.atField(L[1].a)) == p(2);
        3 => return long.view<Pointer>(Pointer.atField(L[1].b)) == p(3);
        _ => return false;
    }

}