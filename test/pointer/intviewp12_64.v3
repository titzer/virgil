//@execute 0=true; 1=true; 2=true
def f1 = 10;
def f2 = [1, 2, 3];
def f3 = "arf";
def p(a: int) -> long {
    match (a) {
        0 => return long.view<Pointer>(Pointer.atField(f1));
        1 => return long.view<Pointer>(Pointer.atField(f2));
        2 => return long.view<Pointer>(Pointer.atField(f3));
        _ => return 42; 
    }
}
def main(a: int) -> bool {
    match (a) {
        0 => return long.view(Pointer.atField(f1)) == p(0);
        1 => return long.view(Pointer.atField(f2)) == p(1);
        2 => return long.view(Pointer.atField(f3)) == p(2);
        _ => return false; 
    }
}