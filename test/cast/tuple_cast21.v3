//@execute 0=0; 1=1; 2=2; 3=3; 4=4
def KB: byte = 44;
def KI: int = 45;
def KL: long = -46;
def KZ: bool = true;

def f<A>(var a: A) -> A {
	var w = (KI, KI, KB);
	if (A.?(w)) a = A.!(w);
	return a;
}
def testBase<A, B>() {
	f(A.default);
}
def test<A, B>() {
	testBase<(A, A, B), (B, A, B)>();
}
def main(a: int) -> int {
	test<int, byte>();
	return a;
}