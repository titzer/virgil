def foo() -> int {
	for (i < cases.length) {
		var t = cases[i];
		var result = double.abs(t.0);
		if (t.2) {  // NaN expected
			if (result == result) return -3 - i;
		} else {
			var expected = t.1;
			if (result != expected) {
				System.puts(Strings.format2("expected %x, got %x\n", u64.view(expected), u64.view(result)));
				return -3 - i;
			}
		}
	}
	return 97;
}

def main() -> int {
	var res = foo();
	System.puti(res);
	System.ln();
	return res;
}

