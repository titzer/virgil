
def ITER = 15000;

def main(a: int) -> int {
    System.puts("==== Running MicroLICM test ====\n");
    var result = 0;
    for (i < ITER) {
        for (j < ITER) {
            // Hoist to the outer loop
            var a = a + 1*i;
            var b = a + 2*i;
            var c = a + 3*i;
            var d = a + 4*i;
            var e = a + 5*i;

            result += a + b + c + d + e;

            // These should be hoisted outside of the outer loop 
            var f = a * 6;
            var g = a * 7;
            var h = a * 8;
            var i = a * 9;
            var j = a * 10;
            result += f + g + h + i + j;
        }
        // These should be hoisted outside of the outer loop 
        var f = a * 6;
        var g = a * 7;
        var h = a * 8;
        var i = a * 9;
        var j = a * 10;
        result += f + g + h + i + j;
    }

    System.puti(result / result);
    System.ln();
    return 0;
    
}