	}
	def r_s_i(name: string, a: X86_64Reg, b: X86_64Xmmr, imm: int, rex: byte) -> this {
		show_pos().puts0(name).putc(' ').reg(a, rex).puts(", ").xreg(b).puts(", ").imm(imm).done();
	}
	def s_reg(name: string, a: X86_64Xmmr, b: X86_64Reg, rex: byte) -> this {
