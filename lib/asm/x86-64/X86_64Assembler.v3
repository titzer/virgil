		show_pos().puts0(name).putc(' ').addr(a, rex).puts(", ").xreg(b).puts(", ").imm(imm).done();
	}
	def reg_s(name: string, a: X86_64Reg, b: X86_64Xmmr, rex: byte) -> this {
