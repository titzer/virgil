// Labels can be used for assembling near and far relative branches.
class X86_64Label {
	var pos = -1;
	var near_uses: List<int>;
	var far_uses: List<int>;
	var num: int;
	new() {
		num = ++X86_64Labels.last_num;
	}
	def bound() -> bool { return pos >= 0; }
}
